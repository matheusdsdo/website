[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mmodule.exports = function bind(fn, thisArg) {[m
[32m+[m[32m  return function wrap() {[m
[32m+[m[32m    var args = new Array(arguments.length);[m
[32m+[m[32m    for (var i = 0; i < args.length; i++) {[m
[32m+[m[32m      args[i] = arguments[i];[m
[32m+[m[32m    }[m
[32m+[m[32m    return fn.apply(thisArg, args);[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/lib/helpers/buildURL.js b/servidor/node_modules/axios/lib/helpers/buildURL.js[m
[1mnew file mode 100644[m
[1mindex 0000000..31595c3[m
