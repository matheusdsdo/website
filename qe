[32m+[m[32m  config: AxiosRequestConfig;[m
[32m+[m[32m  code?: string;[m
[32m+[m[32m  request?: any;[m
[32m+[m[32m  response?: AxiosResponse<T>;[m
[32m+[m[32m  isAxiosError: boolean;[m
[32m+[m[32m  toJSON: () => object;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface AxiosPromise<T = any> extends Promise<AxiosResponse<T>> {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface CancelStatic {[m
[32m+[m[32m  new (message?: string): Cancel;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface Cancel {[m
[32m+[m[32m  message: string;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface Canceler {[m
[32m+[m[32m  (message?: string): void;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface CancelTokenStatic {[m
[32m+[m[32m  new (executor: (cancel: Canceler) => void): CancelToken;[m
[32m+[m[32m  source(): CancelTokenSource;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface CancelToken {[m
[32m+[m[32m  promise: Promise<Cancel>;[m
[32m+[m[32m  reason?: Cancel;[m
[32m+[m[32m  throwIfRequested(): void;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface CancelTokenSource {[m
[32m+[m[32m  token: CancelToken;[m
[32m+[m[32m  cancel: Canceler;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface AxiosInterceptorManager<V> {[m
[32m+[m[32m  use(onFulfilled?: (value: V) => V | Promise<V>, onRejected?: (error: any) => any): number;[m
[32m+[m[32m  eject(id: number): void;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface AxiosInstance {[m
[32m+[m[32m  (config: AxiosRequestConfig): AxiosPromise;[m
[32m+[m[32m  (url: string, config?: AxiosRequestConfig): AxiosPromise;[m
[32m+[m[32m  defaults: AxiosRequestConfig;[m
[32m+[m[32m  interceptors: {[m
[32m+[m[32m    request: AxiosInterceptorManager<AxiosRequestConfig>;[m
[32m+[m[32m    response: AxiosInterceptorManager<AxiosResponse>;[m
[32m+[m[32m  };[m
[32m+[m[32m  getUri(config?: AxiosRequestConfig): string;[m
[32m+[m[32m  request<T = any, R = AxiosResponse<T>> (config: AxiosRequestConfig): Promise<R>;[m
[32m+[m[32m  get<T = any, R = AxiosResponse<T>>(url: string, config?: AxiosRequestConfig): Promise<R>;[m
[32m+[m[32m  delete<T = any, R = AxiosResponse<T>>(url: string, config?: AxiosRequestConfig): Promise<R>;[m
[32m+[m[32m  head<T = any, R = AxiosResponse<T>>(url: string, config?: AxiosRequestConfig): Promise<R>;[m
[32m+[m[32m  options<T = any, R = AxiosResponse<T>>(url: string, config?: AxiosRequestConfig): Promise<R>;[m
[32m+[m[32m  post<T = any, R = AxiosResponse<T>>(url: string, data?: any, config?: AxiosRequestConfig): Promise<R>;[m
[32m+[m[32m  put<T = any, R = AxiosResponse<T>>(url: string, data?: any, config?: AxiosRequestConfig): Promise<R>;[m
[32m+[m[32m  patch<T = any, R = AxiosResponse<T>>(url: string, data?: any, config?: AxiosRequestConfig): Promise<R>;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface AxiosStatic extends AxiosInstance {[m
[32m+[m[32m  create(config?: AxiosRequestConfig): AxiosInstance;[m
[32m+[m[32m  Cancel: CancelStatic;[m
[32m+[m[32m  CancelToken: CancelTokenStatic;[m
[32m+[m[32m  isCancel(value: any): boolean;[m
[32m+[m[32m  all<T>(values: (T | Promise<T>)[]): Promise<T[]>;[m
[32m+[m[32m  spread<T, R>(callback: (...args: T[]) => R): (array: T[]) => R;[m
[32m+[m[32m  isAxiosError(payload: any): payload is AxiosError;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mdeclare const axios: AxiosStatic;[m
[32m+[m
[32m+[m[32mexport default axios;[m
