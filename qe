[36m@@ -0,0 +1,70 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./../utils');[m
[32m+[m
[32m+[m[32mfunction encode(val) {[m
[32m+[m[32m  return encodeURIComponent(val).[m
[32m+[m[32m    replace(/%3A/gi, ':').[m
[32m+[m[32m    replace(/%24/g, '$').[m
[32m+[m[32m    replace(/%2C/gi, ',').[m
[32m+[m[32m    replace(/%20/g, '+').[m
[32m+[m[32m    replace(/%5B/gi, '[').[m
[32m+[m[32m    replace(/%5D/gi, ']');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Build a URL by appending params to the end[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} url The base of the url (e.g., http://www.google.com)[m
[32m+[m[32m * @param {object} [params] The params to be appended[m
[32m+[m[32m * @returns {string} The formatted url[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function buildURL(url, params, paramsSerializer) {[m
[32m+[m[32m  /*eslint no-param-reassign:0*/[m
[32m+[m[32m  if (!params) {[m
[32m+[m[32m    return url;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var serializedParams;[m
[32m+[m[32m  if (paramsSerializer) {[m
[32m+[m[32m    serializedParams = paramsSerializer(params);[m
[32m+[m[32m  } else if (utils.isURLSearchParams(params)) {[m
[32m+[m[32m    serializedParams = params.toString();[m
[32m+[m[32m  } else {[m
[32m+[m[32m    var parts = [];[m
[32m+[m
[32m+[m[32m    utils.forEach(params, function serialize(val, key) {[m
[32m+[m[32m      if (val === null || typeof val === 'undefined') {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (utils.isArray(val)) {[m
[32m+[m[32m        key = key + '[]';[m
[32m+[m[32m      } else {[m
[32m+[m[32m        val = [val];[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      utils.forEach(val, function parseValue(v) {[m
[32m+[m[32m        if (utils.isDate(v)) {[m
[32m+[m[32m          v = v.toISOString();[m
[32m+[m[32m        } else if (utils.isObject(v)) {[m
[32m+[m[32m          v = JSON.stringify(v);[m
[32m+[m[32m        }[m
[32m+[m[32m        parts.push(encode(key) + '=' + encode(v));[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    serializedParams = parts.join('&');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (serializedParams) {[m
[32m+[m[32m    var hashmarkIndex = url.indexOf('#');[m
[32m+[m[32m    if (hashmarkIndex !== -1) {[m
[32m+[m[32m      url = url.slice(0, hashmarkIndex);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    url += (url.indexOf('?') === -1 ? '?' : '&') + serializedParams;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return url;[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/lib/helpers/combineURLs.js b/servidor/node_modules/axios/lib/helpers/combineURLs.js[m
[1mnew file mode 100644[m
[1mindex 0000000..f1b58a5[m
