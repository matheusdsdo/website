[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar Cancel = require('./Cancel');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * A `CancelToken` is an object that can be used to request cancellation of an operation.[m
[32m+[m[32m *[m
[32m+[m[32m * @class[m
[32m+[m[32m * @param {Function} executor The executor function.[m
[32m+[m[32m */[m
[32m+[m[32mfunction CancelToken(executor) {[m
[32m+[m[32m  if (typeof executor !== 'function') {[m
[32m+[m[32m    throw new TypeError('executor must be a function.');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var resolvePromise;[m
[32m+[m[32m  this.promise = new Promise(function promiseExecutor(resolve) {[m
[32m+[m[32m    resolvePromise = resolve;[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  var token = this;[m
[32m+[m[32m  executor(function cancel(message) {[m
[32m+[m[32m    if (token.reason) {[m
[32m+[m[32m      // Cancellation has already been requested[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    token.reason = new Cancel(message);[m
[32m+[m[32m    resolvePromise(token.reason);[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Throws a `Cancel` if cancellation has been requested.[m
[32m+[m[32m */[m
[32m+[m[32mCancelToken.prototype.throwIfRequested = function throwIfRequested() {[m
[32m+[m[32m  if (this.reason) {[m
[32m+[m[32m    throw this.reason;[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns an object that contains a new `CancelToken` and a function that, when called,[m
[32m+[m[32m * cancels the `CancelToken`.[m
[32m+[m[32m */[m
[32m+[m[32mCancelToken.source = function source() {[m
[32m+[m[32m  var cancel;[m
[32m+[m[32m  var token = new CancelToken(function executor(c) {[m
[32m+[m[32m    cancel = c;[m
[32m+[m[32m  });[m
[32m+[m[32m  return {[m
[32m+[m[32m    token: token,[m
[32m+[m[32m    cancel: cancel[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = CancelToken;[m
[1mdiff --git a/servidor/node_modules/axios/lib/cancel/isCancel.js b/servidor/node_modules/axios/lib/cancel/isCancel.js[m
[1mnew file mode 100644[m
[1mindex 0000000..051f3ae[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/cancel/isCancel.js[m
[36m@@ -0,0 +1,5 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mmodule.exports = function isCancel(value) {[m
[32m+[m[32m  return !!(value && value.__CANCEL__);[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/lib/core/Axios.js b/servidor/node_modules/axios/lib/core/Axios.js[m
[1mnew file mode 100644[m
[1mindex 0000000..c28c413[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/core/Axios.js[m
[36m@@ -0,0 +1,95 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./../utils');[m
[32m+[m[32mvar buildURL = require('../helpers/buildURL');[m
[32m+[m[32mvar InterceptorManager = require('./InterceptorManager');[m
[32m+[m[32mvar dispatchRequest = require('./dispatchRequest');[m
[32m+[m[32mvar mergeConfig = require('./mergeConfig');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a new instance of Axios[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} instanceConfig The default config for the instance[m
[32m+[m[32m */[m
[32m+[m[32mfunction Axios(instanceConfig) {[m
[32m+[m[32m  this.defaults = instanceConfig;[m
[32m+[m[32m  this.interceptors = {[m
[32m+[m[32m    request: new InterceptorManager(),[m
[32m+[m[32m    response: new InterceptorManager()[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Dispatch a request[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} config The config specific for this request (merged with this.defaults)[m
[32m+[m[32m */[m
[32m+[m[32mAxios.prototype.request = function request(config) {[m
[32m+[m[32m  /*eslint no-param-reassign:0*/[m
[32m+[m[32m  // Allow for axios('example/url'[, config]) a la fetch API[m
[32m+[m[32m  if (typeof config === 'string') {[m
[32m+[m[32m    config = arguments[1] || {};[m
[32m+[m[32m    config.url = arguments[0];[m
[32m+[m[32m  } else {[m
[32m+[m[32m    config = config || {};[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  config = mergeConfig(this.defaults, config);[m
[32m+[m
[32m+[m[32m  // Set config.method[m
[32m+[m[32m  if (config.method) {[m
[32m+[m[32m    config.method = config.method.toLowerCase();[m
[32m+[m[32m  } else if (this.defaults.method) {[m
[32m+[m[32m    config.method = this.defaults.method.toLowerCase();[m
[32m+[m[32m  } else {[m
[32m+[m[32m    config.method = 'get';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Hook up interceptors middleware[m
[32m+[m[32m  var chain = [dispatchRequest, undefined];[m
[32m+[m[32m  var promise = Promise.resolve(config);[m
[32m+[m
[32m+[m[32m  this.interceptors.request.forEach(function unshiftRequestInterceptors(interceptor) {[m
[32m+[m[32m    chain.unshift(interceptor.fulfilled, interceptor.rejected);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  this.interceptors.response.forEach(function pushResponseInterceptors(interceptor) {[m
[32m+[m[32m    chain.push(interceptor.fulfilled, interceptor.rejected);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  while (chain.length) {[m
[32m+[m[32m    promise = promise.then(chain.shift(), chain.shift());[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return promise;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mAxios.prototype.getUri = function getUri(config) {[m
[32m+[m[32m  config = mergeConfig(this.defaults, config);[m
[32m+[m[32m  return buildURL(config.url, config.params, config.paramsSerializer).replace(/^\?/, '');[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Provide aliases for supported request methods[m
[32m+[m[32mutils.forEach(['delete', 'get', 'head', 'options'], function forEachMethodNoData(method) {[m
[32m+[m[32m  /*eslint func-names:0*/[m
[32m+[m[32m  Axios.prototype[method] = function(url, config) {[m
[32m+[m[32m    return this.request(mergeConfig(config || {}, {[m
[32m+[m[32m      method: method,[m
[32m+[m[32m      url: url,[m
[32m+[m[32m      data: (config || {}).data[m
[32m+[m[32m    }));[m
[32m+[m[32m  };[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mutils.forEach(['post', 'put', 'patch'], function forEachMethodWithData(method) {[m
[32m+[m[32m  /*eslint func-names:0*/[m
[32m+[m[32m  Axios.prototype[method] = function(url, data, config) {[m
[32m+[m[32m    return this.request(mergeConfig(config || {}, {[m
[32m+[m[32m      method: method,[m
[32m+[m[32m      url: url,[m
[32m+[m[32m      data: data[m
[32m+[m[32m    }));[m
[32m+[m[32m  };[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mmodule.exports = Axios;[m
