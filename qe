[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * A `Cancel` is an object that is thrown when an operation is canceled.[m
[32m+[m[32m *[m
[32m+[m[32m * @class[m
[32m+[m[32m * @param {string=} message The message.[m
[32m+[m[32m */[m
[32m+[m[32mfunction Cancel(message) {[m
[32m+[m[32m  this.message = message;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mCancel.prototype.toString = function toString() {[m
[32m+[m[32m  return 'Cancel' + (this.message ? ': ' + this.message : '');[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mCancel.prototype.__CANCEL__ = true;[m
[32m+[m
[32m+[m[32mmodule.exports = Cancel;[m
