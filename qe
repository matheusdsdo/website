[36m@@ -0,0 +1,25 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar createError = require('./createError');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Resolve or reject a Promise based on response status.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Function} resolve A function that resolves the promise.[m
[32m+[m[32m * @param {Function} reject A function that rejects the promise.[m
[32m+[m[32m * @param {object} response The response.[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function settle(resolve, reject, response) {[m
[32m+[m[32m  var validateStatus = response.config.validateStatus;[m
[32m+[m[32m  if (!response.status || !validateStatus || validateStatus(response.status)) {[m
[32m+[m[32m    resolve(response);[m
[32m+[m[32m  } else {[m
[32m+[m[32m    reject(createError([m
[32m+[m[32m      'Request failed with status code ' + response.status,[m
[32m+[m[32m      response.config,[m
[32m+[m[32m      null,[m
[32m+[m[32m      response.request,[m
[32m+[m[32m      response[m
[32m+[m[32m    ));[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
