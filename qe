[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar bind = require('./helpers/bind');[m
[32m+[m
[32m+[m[32m/*global toString:true*/[m
[32m+[m
[32m+[m[32m// utils is a library of generic helper functions non-specific to axios[m
[32m+[m
[32m+[m[32mvar toString = Object.prototype.toString;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is an Array[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is an Array, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isArray(val) {[m
[32m+[m[32m  return toString.call(val) === '[object Array]';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is undefined[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if the value is undefined, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isUndefined(val) {[m
[32m+[m[32m  return typeof val === 'undefined';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a Buffer[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a Buffer, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isBuffer(val) {[m
[32m+[m[32m  return val !== null && !isUndefined(val) && val.constructor !== null && !isUndefined(val.constructor)[m
[32m+[m[32m    && typeof val.constructor.isBuffer === 'function' && val.constructor.isBuffer(val);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is an ArrayBuffer[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is an ArrayBuffer, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isArrayBuffer(val) {[m
[32m+[m[32m  return toString.call(val) === '[object ArrayBuffer]';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a FormData[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is an FormData, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isFormData(val) {[m
[32m+[m[32m  return (typeof FormData !== 'undefined') && (val instanceof FormData);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a view on an ArrayBuffer[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a view on an ArrayBuffer, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isArrayBufferView(val) {[m
[32m+[m[32m  var result;[m
[32m+[m[32m  if ((typeof ArrayBuffer !== 'undefined') && (ArrayBuffer.isView)) {[m
[32m+[m[32m    result = ArrayBuffer.isView(val);[m
[32m+[m[32m  } else {[m
[32m+[m[32m    result = (val) && (val.buffer) && (val.buffer instanceof ArrayBuffer);[m
[32m+[m[32m  }[m
[32m+[m[32m  return result;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a String[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a String, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isString(val) {[m
[32m+[m[32m  return typeof val === 'string';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a Number[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a Number, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isNumber(val) {[m
[32m+[m[32m  return typeof val === 'number';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is an Object[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is an Object, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isObject(val) {[m
[32m+[m[32m  return val !== null && typeof val === 'object';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a plain Object[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @return {boolean} True if value is a plain Object, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isPlainObject(val) {[m
[32m+[m[32m  if (toString.call(val) !== '[object Object]') {[m
[32m+[m[32m    return false;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var prototype = Object.getPrototypeOf(val);[m
[32m+[m[32m  return prototype === null || prototype === Object.prototype;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a Date[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a Date, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isDate(val) {[m
[32m+[m[32m  return toString.call(val) === '[object Date]';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a File[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a File, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isFile(val) {[m
[32m+[m[32m  return toString.call(val) === '[object File]';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a Blob[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a Blob, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isBlob(val) {[m
[32m+[m[32m  return toString.call(val) === '[object Blob]';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a Function[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a Function, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isFunction(val) {[m
[32m+[m[32m  return toString.call(val) === '[object Function]';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a Stream[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a Stream, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isStream(val) {[m
[32m+[m[32m  return isObject(val) && isFunction(val.pipe);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if a value is a URLSearchParams object[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} val The value to test[m
[32m+[m[32m * @returns {boolean} True if value is a URLSearchParams object, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mfunction isURLSearchParams(val) {[m
[32m+[m[32m  return typeof URLSearchParams !== 'undefined' && val instanceof URLSearchParams;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Trim excess whitespace off the beginning and end of a string[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} str The String to trim[m
[32m+[m[32m * @returns {String} The String freed of excess whitespace[m
[32m+[m[32m */[m
[32m+[m[32mfunction trim(str) {[m
[32m+[m[32m  return str.replace(/^\s*/, '').replace(/\s*$/, '');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if we're running in a standard browser environment[m
[32m+[m[32m *[m
[32m+[m[32m * This allows axios to run in a web worker, and react-native.[m
[32m+[m[32m * Both environments support XMLHttpRequest, but not fully standard globals.[m
[32m+[m[32m *[m
[32m+[m[32m * web workers:[m
[32m+[m[32m *  typeof window -> undefined[m
[32m+[m[32m *  typeof document -> undefined[m
[32m+[m[32m *[m
[32m+[m[32m * react-native:[m
[32m+[m[32m *  navigator.product -> 'ReactNative'[m
[32m+[m[32m * nativescript[m
[32m+[m[32m *  navigator.product -> 'NativeScript' or 'NS'[m
[32m+[m[32m */[m
[32m+[m[32mfunction isStandardBrowserEnv() {[m
[32m+[m[32m  if (typeof navigator !== 'undefined' && (navigator.product === 'ReactNative' ||[m
[32m+[m[32m                                           navigator.product === 'NativeScript' ||[m
[32m+[m[32m                                           navigator.product === 'NS')) {[m
[32m+[m[32m    return false;[m
[32m+[m[32m  }[m
[32m+[m[32m  return ([m
[32m+[m[32m    typeof window !== 'undefined' &&[m
[32m+[m[32m    typeof document !== 'undefined'[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Iterate over an Array or an Object invoking a function for each item.[m
[32m+[m[32m *[m
[32m+[m[32m * If `obj` is an Array callback will be called passing[m
[32m+[m[32m * the value, index, and complete array for each item.[m
[32m+[m[32m *[m
[32m+[m[32m * If 'obj' is an Object callback will be called passing[m
[32m+[m[32m * the value, key, and complete object for each property.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object|Array} obj The object to iterate[m
[32m+[m[32m * @param {Function} fn The callback to invoke for each item[m
[32m+[m[32m */[m
[32m+[m[32mfunction forEach(obj, fn) {[m
[32m+[m[32m  // Don't bother if no value provided[m
[32m+[m[32m  if (obj === null || typeof obj === 'undefined') {[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Force an array if not already something iterable[m
[32m+[m[32m  if (typeof obj !== 'object') {[m
[32m+[m[32m    /*eslint no-param-reassign:0*/[m
[32m+[m[32m    obj = [obj];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (isArray(obj)) {[m
[32m+[m[32m    // Iterate over array values[m
[32m+[m[32m    for (var i = 0, l = obj.length; i < l; i++) {[m
[32m+[m[32m      fn.call(null, obj[i], i, obj);[m
[32m+[m[32m    }[m
[32m+[m[32m  } else {[m
[32m+[m[32m    // Iterate over object keys[m
[32m+[m[32m    for (var key in obj) {[m
[32m+[m[32m      if (Object.prototype.hasOwnProperty.call(obj, key)) {[m
[32m+[m[32m        fn.call(null, obj[key], key, obj);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Accepts varargs expecting each argument to be an object, then[m
[32m+[m[32m * immutably merges the properties of each object and returns result.[m
[32m+[m[32m *[m
[32m+[m[32m * When multiple objects contain the same key the later object in[m
[32m+[m[32m * the arguments list will take precedence.[m
[32m+[m[32m *[m
[32m+[m[32m * Example:[m
[32m+[m[32m *[m
[32m+[m[32m * ```js[m
[32m+[m[32m * var result = merge({foo: 123}, {foo: 456});[m
[32m+[m[32m * console.log(result.foo); // outputs 456[m
[32m+[m[32m * ```[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} obj1 Object to merge[m
[32m+[m[32m * @returns {Object} Result of all merge properties[m
[32m+[m[32m */[m
[32m+[m[32mfunction merge(/* obj1, obj2, obj3, ... */) {[m
[32m+[m[32m  var result = {};[m
[32m+[m[32m  function assignValue(val, key) {[m
[32m+[m[32m    if (isPlainObject(result[key]) && isPlainObject(val)) {[m
[32m+[m[32m      result[key] = merge(result[key], val);[m
[32m+[m[32m    } else if (isPlainObject(val)) {[m
[32m+[m[32m      result[key] = merge({}, val);[m
[32m+[m[32m    } else if (isArray(val)) {[m
[32m+[m[32m      result[key] = val.slice();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      result[key] = val;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (var i = 0, l = arguments.length; i < l; i++) {[m
[32m+[m[32m    forEach(arguments[i], assignValue);[m
[32m+[m[32m  }[m
[32m+[m[32m  return result;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Extends object a by mutably adding to it the properties of object b.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} a The object to be extended[m
[32m+[m[32m * @param {Object} b The object to copy properties from[m
[32m+[m[32m * @param {Object} thisArg The object to bind function to[m
[32m+[m[32m * @return {Object} The resulting value of object a[m
[32m+[m[32m */[m
[32m+[m[32mfunction extend(a, b, thisArg) {[m
[32m+[m[32m  forEach(b, function assignValue(val, key) {[m
[32m+[m[32m    if (thisArg && typeof val === 'function') {[m
[32m+[m[32m      a[key] = bind(val, thisArg);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      a[key] = val;[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m  return a;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Remove byte order marker. This catches EF BB BF (the UTF-8 BOM)[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} content with BOM[m
[32m+[m[32m * @return {string} content value without BOM[m
[32m+[m[32m */[m
[32m+[m[32mfunction stripBOM(content) {[m
[32m+[m[32m  if (content.charCodeAt(0) === 0xFEFF) {[m
[32m+[m[32m    content = content.slice(1);[m
[32m+[m[32m  }[m
[32m+[m[32m  return content;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m  isArray: isArray,[m
[32m+[m[32m  isArrayBuffer: isArrayBuffer,[m
[32m+[m[32m  isBuffer: isBuffer,[m
[32m+[m[32m  isFormData: isFormData,[m
[32m+[m[32m  isArrayBufferView: isArrayBufferView,[m
[32m+[m[32m  isString: isString,[m
[32m+[m[32m  isNumber: isNumber,[m
[32m+[m[32m  isObject: isObject,[m
[32m+[m[32m  isPlainObject: isPlainObject,[m
[32m+[m[32m  isUndefined: isUndefined,[m
[32m+[m[32m  isDate: isDate,[m
[32m+[m[32m  isFile: isFile,[m
[32m+[m[32m  isBlob: isBlob,[m
[32m+[m[32m  isFunction: isFunction,[m
[32m+[m[32m  isStream: isStream,[m
[32m+[m[32m  isURLSearchParams: isURLSearchParams,[m
[32m+[m[32m  isStandardBrowserEnv: isStandardBrowserEnv,[m
[32m+[m[32m  forEach: forEach,[m
[32m+[m[32m  merge: merge,[m
[32m+[m[32m  extend: extend,[m
[32m+[m[32m  trim: trim,[m
[32m+[m[32m  stripBOM: stripBOM[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/package.json b/servidor/node_modules/axios/package.json[m
