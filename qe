[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create an Error with the specified message, config, error code, request and response.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} message The error message.[m
[32m+[m[32m * @param {Object} config The config.[m
[32m+[m[32m * @param {string} [code] The error code (for example, 'ECONNABORTED').[m
[32m+[m[32m * @param {Object} [request] The request.[m
[32m+[m[32m * @param {Object} [response] The response.[m
[32m+[m[32m * @returns {Error} The created error.[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function createError(message, config, code, request, response) {[m
[32m+[m[32m  var error = new Error(message);[m
[32m+[m[32m  return enhanceError(error, config, code, request, response);[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/lib/core/dispatchRequest.js b/servidor/node_modules/axios/lib/core/dispatchRequest.js[m
[1mnew file mode 100644[m
[1mindex 0000000..c8267ad[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/core/dispatchRequest.js[m
[36m@@ -0,0 +1,79 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./../utils');[m
[32m+[m[32mvar transformData = require('./transformData');[m
[32m+[m[32mvar isCancel = require('../cancel/isCancel');[m
[32m+[m[32mvar defaults = require('../defaults');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Throws a `Cancel` if cancellation has been requested.[m
[32m+[m[32m */[m
[32m+[m[32mfunction throwIfCancellationRequested(config) {[m
[32m+[m[32m  if (config.cancelToken) {[m
[32m+[m[32m    config.cancelToken.throwIfRequested();[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Dispatch a request to the server using the configured adapter.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} config The config that is to be used for the request[m
[32m+[m[32m * @returns {Promise} The Promise to be fulfilled[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function dispatchRequest(config) {[m
[32m+[m[32m  throwIfCancellationRequested(config);[m
[32m+[m
[32m+[m[32m  // Ensure headers exist[m
[32m+[m[32m  config.headers = config.headers || {};[m
[32m+[m
[32m+[m[32m  // Transform request data[m
[32m+[m[32m  config.data = transformData([m
[32m+[m[32m    config.data,[m
[32m+[m[32m    config.headers,[m
[32m+[m[32m    config.transformRequest[m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  // Flatten headers[m
[32m+[m[32m  config.headers = utils.merge([m
[32m+[m[32m    config.headers.common || {},[m
[32m+[m[32m    config.headers[config.method] || {},[m
[32m+[m[32m    config.headers[m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  utils.forEach([m
[32m+[m[32m    ['delete', 'get', 'head', 'post', 'put', 'patch', 'common'],[m
[32m+[m[32m    function cleanHeaderConfig(method) {[m
[32m+[m[32m      delete config.headers[method];[m
[32m+[m[32m    }[m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  var adapter = config.adapter || defaults.adapter;[m
[32m+[m
[32m+[m[32m  return adapter(config).then(function onAdapterResolution(response) {[m
[32m+[m[32m    throwIfCancellationRequested(config);[m
[32m+[m
[32m+[m[32m    // Transform response data[m
[32m+[m[32m    response.data = transformData([m
[32m+[m[32m      response.data,[m
[32m+[m[32m      response.headers,[m
[32m+[m[32m      config.transformResponse[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    return response;[m
[32m+[m[32m  }, function onAdapterRejection(reason) {[m
[32m+[m[32m    if (!isCancel(reason)) {[m
[32m+[m[32m      throwIfCancellationRequested(config);[m
[32m+[m
[32m+[m[32m      // Transform response data[m
[32m+[m[32m      if (reason && reason.response) {[m
[32m+[m[32m        reason.response.data = transformData([m
[32m+[m[32m          reason.response.data,[m
[32m+[m[32m          reason.response.headers,[m
[32m+[m[32m          config.transformResponse[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return Promise.reject(reason);[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/lib/core/enhanceError.js b/servidor/node_modules/axios/lib/core/enhanceError.js[m
[1mnew file mode 100644[m
[1mindex 0000000..b6bc444[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/core/enhanceError.js[m
[36m@@ -0,0 +1,42 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Update an Error with the specified config, error code, and response.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Error} error The error to update.[m
[32m+[m[32m * @param {Object} config The config.[m
[32m+[m[32m * @param {string} [code] The error code (for example, 'ECONNABORTED').[m
[32m+[m[32m * @param {Object} [request] The request.[m
[32m+[m[32m * @param {Object} [response] The response.[m
[32m+[m[32m * @returns {Error} The error.[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function enhanceError(error, config, code, request, response) {[m
[32m+[m[32m  error.config = config;[m
[32m+[m[32m  if (code) {[m
[32m+[m[32m    error.code = code;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  error.request = request;[m
[32m+[m[32m  error.response = response;[m
[32m+[m[32m  error.isAxiosError = true;[m
[32m+[m
[32m+[m[32m  error.toJSON = function toJSON() {[m
[32m+[m[32m    return {[m
[32m+[m[32m      // Standard[m
[32m+[m[32m      message: this.message,[m
[32m+[m[32m      name: this.name,[m
[32m+[m[32m      // Microsoft[m
[32m+[m[32m      description: this.description,[m
[32m+[m[32m      number: this.number,[m
[32m+[m[32m      // Mozilla[m
[32m+[m[32m      fileName: this.fileName,[m
[32m+[m[32m      lineNumber: this.lineNumber,[m
[32m+[m[32m      columnNumber: this.columnNumber,[m
[32m+[m[32m      stack: this.stack,[m
[32m+[m[32m      // Axios[m
[32m+[m[32m      config: this.config,[m
[32m+[m[32m      code: this.code[m
[32m+[m[32m    };[m
[32m+[m[32m  };[m
[32m+[m[32m  return error;[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/lib/core/mergeConfig.js b/servidor/node_modules/axios/lib/core/mergeConfig.js[m
[1mnew file mode 100644[m
[1mindex 0000000..5a2c10c[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/core/mergeConfig.js[m
[36m@@ -0,0 +1,87 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('../utils');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Config-specific merge-function which creates a new config-object[m
[32m+[m[32m * by merging two configuration objects together.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} config1[m
[32m+[m[32m * @param {Object} config2[m
[32m+[m[32m * @returns {Object} New object resulting from merging config2 to config1[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function mergeConfig(config1, config2) {[m
[32m+[m[32m  // eslint-disable-next-line no-param-reassign[m
[32m+[m[32m  config2 = config2 || {};[m
[32m+[m[32m  var config = {};[m
[32m+[m
[32m+[m[32m  var valueFromConfig2Keys = ['url', 'method', 'data'];[m
[32m+[m[32m  var mergeDeepPropertiesKeys = ['headers', 'auth', 'proxy', 'params'];[m
[32m+[m[32m  var defaultToConfig2Keys = [[m
[32m+[m[32m    'baseURL', 'transformRequest', 'transformResponse', 'paramsSerializer',[m
[32m+[m[32m    'timeout', 'timeoutMessage', 'withCredentials', 'adapter', 'responseType', 'xsrfCookieName',[m
[32m+[m[32m    'xsrfHeaderName', 'onUploadProgress', 'onDownloadProgress', 'decompress',[m
[32m+[m[32m    'maxContentLength', 'maxBodyLength', 'maxRedirects', 'transport', 'httpAgent',[m
[32m+[m[32m    'httpsAgent', 'cancelToken', 'socketPath', 'responseEncoding'[m
[32m+[m[32m  ];[m
[32m+[m[32m  var directMergeKeys = ['validateStatus'];[m
[32m+[m
[32m+[m[32m  function getMergedValue(target, source) {[m
[32m+[m[32m    if (utils.isPlainObject(target) && utils.isPlainObject(source)) {[m
[32m+[m[32m      return utils.merge(target, source);[m
[32m+[m[32m    } else if (utils.isPlainObject(source)) {[m
[32m+[m[32m      return utils.merge({}, source);[m
[32m+[m[32m    } else if (utils.isArray(source)) {[m
[32m+[m[32m      return source.slice();[m
[32m+[m[32m    }[m
[32m+[m[32m    return source;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function mergeDeepProperties(prop) {[m
[32m+[m[32m    if (!utils.isUndefined(config2[prop])) {[m
[32m+[m[32m      config[prop] = getMergedValue(config1[prop], config2[prop]);[m
[32m+[m[32m    } else if (!utils.isUndefined(config1[prop])) {[m
[32m+[m[32m      config[prop] = getMergedValue(undefined, config1[prop]);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  utils.forEach(valueFromConfig2Keys, function valueFromConfig2(prop) {[m
[32m+[m[32m    if (!utils.isUndefined(config2[prop])) {[m
[32m+[m[32m      config[prop] = getMergedValue(undefined, config2[prop]);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  utils.forEach(mergeDeepPropertiesKeys, mergeDeepProperties);[m
[32m+[m
[32m+[m[32m  utils.forEach(defaultToConfig2Keys, function defaultToConfig2(prop) {[m
[32m+[m[32m    if (!utils.isUndefined(config2[prop])) {[m
[32m+[m[32m      config[prop] = getMergedValue(undefined, config2[prop]);[m
[32m+[m[32m    } else if (!utils.isUndefined(config1[prop])) {[m
[32m+[m[32m      config[prop] = getMergedValue(undefined, config1[prop]);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  utils.forEach(directMergeKeys, function merge(prop) {[m
[32m+[m[32m    if (prop in config2) {[m
[32m+[m[32m      config[prop] = getMergedValue(config1[prop], config2[prop]);[m
[32m+[m[32m    } else if (prop in config1) {[m
[32m+[m[32m      config[prop] = getMergedValue(undefined, config1[prop]);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  var axiosKeys = valueFromConfig2Keys[m
[32m+[m[32m    .concat(mergeDeepPropertiesKeys)[m
[32m+[m[32m    .concat(defaultToConfig2Keys)[m
[32m+[m[32m    .concat(directMergeKeys);[m
[32m+[m
[32m+[m[32m  var otherKeys = Object[m
[32m+[m[32m    .keys(config1)[m
[32m+[m[32m    .concat(Object.keys(config2))[m
[32m+[m[32m    .filter(function filterAxiosKeys(key) {[m
[32m+[m[32m      return axiosKeys.indexOf(key) === -1;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m  utils.forEach(otherKeys, mergeDeepProperties);[m
[32m+[m
[32m+[m[32m  return config;[m
[32m+[m[32m};[m
