[1mnew file mode 100644[m
[1mindex 0000000..3027752[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/adapters/xhr.js[m
[36m@@ -0,0 +1,179 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./../utils');[m
[32m+[m[32mvar settle = require('./../core/settle');[m
[32m+[m[32mvar cookies = require('./../helpers/cookies');[m
[32m+[m[32mvar buildURL = require('./../helpers/buildURL');[m
[32m+[m[32mvar buildFullPath = require('../core/buildFullPath');[m
[32m+[m[32mvar parseHeaders = require('./../helpers/parseHeaders');[m
[32m+[m[32mvar isURLSameOrigin = require('./../helpers/isURLSameOrigin');[m
[32m+[m[32mvar createError = require('../core/createError');[m
[32m+[m
[32m+[m[32mmodule.exports = function xhrAdapter(config) {[m
[32m+[m[32m  return new Promise(function dispatchXhrRequest(resolve, reject) {[m
[32m+[m[32m    var requestData = config.data;[m
[32m+[m[32m    var requestHeaders = config.headers;[m
[32m+[m
[32m+[m[32m    if (utils.isFormData(requestData)) {[m
[32m+[m[32m      delete requestHeaders['Content-Type']; // Let the browser set it[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var request = new XMLHttpRequest();[m
[32m+[m
[32m+[m[32m    // HTTP basic authentication[m
[32m+[m[32m    if (config.auth) {[m
[32m+[m[32m      var username = config.auth.username || '';[m
[32m+[m[32m      var password = config.auth.password ? unescape(encodeURIComponent(config.auth.password)) : '';[m
[32m+[m[32m      requestHeaders.Authorization = 'Basic ' + btoa(username + ':' + password);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var fullPath = buildFullPath(config.baseURL, config.url);[m
[32m+[m[32m    request.open(config.method.toUpperCase(), buildURL(fullPath, config.params, config.paramsSerializer), true);[m
[32m+[m
[32m+[m[32m    // Set the request timeout in MS[m
[32m+[m[32m    request.timeout = config.timeout;[m
[32m+[m
[32m+[m[32m    // Listen for ready state[m
[32m+[m[32m    request.onreadystatechange = function handleLoad() {[m
[32m+[m[32m      if (!request || request.readyState !== 4) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // The request errored out and we didn't get a response, this will be[m
[32m+[m[32m      // handled by onerror instead[m
[32m+[m[32m      // With one exception: request that using file: protocol, most browsers[m
[32m+[m[32m      // will return status as 0 even though it's a successful request[m
[32m+[m[32m      if (request.status === 0 && !(request.responseURL && request.responseURL.indexOf('file:') === 0)) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Prepare the response[m
[32m+[m[32m      var responseHeaders = 'getAllResponseHeaders' in request ? parseHeaders(request.getAllResponseHeaders()) : null;[m
[32m+[m[32m      var responseData = !config.responseType || config.responseType === 'text' ? request.responseText : request.response;[m
[32m+[m[32m      var response = {[m
[32m+[m[32m        data: responseData,[m
[32m+[m[32m        status: request.status,[m
[32m+[m[32m        statusText: request.statusText,[m
[32m+[m[32m        headers: responseHeaders,[m
[32m+[m[32m        config: config,[m
[32m+[m[32m        request: request[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      settle(resolve, reject, response);[m
[32m+[m
[32m+[m[32m      // Clean up request[m
[32m+[m[32m      request = null;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Handle browser request cancellation (as opposed to a manual cancellation)[m
[32m+[m[32m    request.onabort = function handleAbort() {[m
[32m+[m[32m      if (!request) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      reject(createError('Request aborted', config, 'ECONNABORTED', request));[m
[32m+[m
[32m+[m[32m      // Clean up request[m
[32m+[m[32m      request = null;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Handle low level network errors[m
[32m+[m[32m    request.onerror = function handleError() {[m
[32m+[m[32m      // Real errors are hidden from us by the browser[m
[32m+[m[32m      // onerror should only fire if it's a network error[m
[32m+[m[32m      reject(createError('Network Error', config, null, request));[m
[32m+[m
[32m+[m[32m      // Clean up request[m
[32m+[m[32m      request = null;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Handle timeout[m
[32m+[m[32m    request.ontimeout = function handleTimeout() {[m
[32m+[m[32m      var timeoutErrorMessage = 'timeout of ' + config.timeout + 'ms exceeded';[m
[32m+[m[32m      if (config.timeoutErrorMessage) {[m
[32m+[m[32m        timeoutErrorMessage = config.timeoutErrorMessage;[m
[32m+[m[32m      }[m
[32m+[m[32m      reject(createError(timeoutErrorMessage, config, 'ECONNABORTED',[m
[32m+[m[32m        request));[m
[32m+[m
[32m+[m[32m      // Clean up request[m
[32m+[m[32m      request = null;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Add xsrf header[m
[32m+[m[32m    // This is only done if running in a standard browser environment.[m
[32m+[m[32m    // Specifically not if we're in a web worker, or react-native.[m
[32m+[m[32m    if (utils.isStandardBrowserEnv()) {[m
[32m+[m[32m      // Add xsrf header[m
[32m+[m[32m      var xsrfValue = (config.withCredentials || isURLSameOrigin(fullPath)) && config.xsrfCookieName ?[m
[32m+[m[32m        cookies.read(config.xsrfCookieName) :[m
[32m+[m[32m        undefined;[m
[32m+[m
[32m+[m[32m      if (xsrfValue) {[m
[32m+[m[32m        requestHeaders[config.xsrfHeaderName] = xsrfValue;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Add headers to the request[m
[32m+[m[32m    if ('setRequestHeader' in request) {[m
[32m+[m[32m      utils.forEach(requestHeaders, function setRequestHeader(val, key) {[m
[32m+[m[32m        if (typeof requestData === 'undefined' && key.toLowerCase() === 'content-type') {[m
[32m+[m[32m          // Remove Content-Type if data is undefined[m
[32m+[m[32m          delete requestHeaders[key];[m
[32m+[m[32m        } else {[m
[32m+[m[32m          // Otherwise add header to the request[m
[32m+[m[32m          request.setRequestHeader(key, val);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Add withCredentials to request if needed[m
[32m+[m[32m    if (!utils.isUndefined(config.withCredentials)) {[m
[32m+[m[32m      request.withCredentials = !!config.withCredentials;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Add responseType to request if needed[m
[32m+[m[32m    if (config.responseType) {[m
[32m+[m[32m      try {[m
[32m+[m[32m        request.responseType = config.responseType;[m
[32m+[m[32m      } catch (e) {[m
[32m+[m[32m        // Expected DOMException thrown by browsers not compatible XMLHttpRequest Level 2.[m
[32m+[m[32m        // But, this can be suppressed for 'json' type as it can be parsed by default 'transformResponse' function.[m
[32m+[m[32m        if (config.responseType !== 'json') {[m
[32m+[m[32m          throw e;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Handle progress if needed[m
[32m+[m[32m    if (typeof config.onDownloadProgress === 'function') {[m
[32m+[m[32m      request.addEventListener('progress', config.onDownloadProgress);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Not all browsers support upload events[m
[32m+[m[32m    if (typeof config.onUploadProgress === 'function' && request.upload) {[m
[32m+[m[32m      request.upload.addEventListener('progress', config.onUploadProgress);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (config.cancelToken) {[m
[32m+[m[32m      // Handle cancellation[m
[32m+[m[32m      config.cancelToken.promise.then(function onCanceled(cancel) {[m
[32m+[m[32m        if (!request) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        request.abort();[m
[32m+[m[32m        reject(cancel);[m
[32m+[m[32m        // Clean up request[m
[32m+[m[32m        request = null;[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!requestData) {[m
[32m+[m[32m      requestData = null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Send the request[m
[32m+[m[32m    request.send(requestData);[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
