[1m+++ b/servidor/node_modules/axios/lib/helpers/isAbsoluteURL.js[m
[36m@@ -0,0 +1,14 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determines whether the specified URL is absolute[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} url The URL to test[m
[32m+[m[32m * @returns {boolean} True if the specified URL is absolute, otherwise false[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function isAbsoluteURL(url) {[m
[32m+[m[32m  // A URL is considered absolute if it begins with "<scheme>://" or "//" (protocol-relative URL).[m
[32m+[m[32m  // RFC 3986 defines scheme name as a sequence of characters beginning with a letter and followed[m
[32m+[m[32m  // by any combination of letters, digits, plus, period, or hyphen.[m
[32m+[m[32m  return /^([a-z][a-z\d\+\-\.]*:)?\/\//i.test(url);[m
[32m+[m[32m};[m
