[32m+[m[32mimport React from 'react';[m
[32m+[m[32mimport Axios from 'axios';[m
[32m+[m[32mimport { useState } from "react";[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mfunction Usuarios(){[m
[32m+[m[32m/*[m
[32m+[m[32m    const [codigo,setCodigo] = useState('')[m
[32m+[m[32m    const [usuario, setUsuario] = useState('')[m
[32m+[m[32m    const [senha, setSenha] = useState('')[m
[32m+[m[32m    const [email, setEmail] = useState('')[m
[32m+[m[32m  */[m[41m  [m
[32m+[m[32m    const [listaUsuarios, setListaUsuarios] = useState([]);[m
[32m+[m
[32m+[m[32m    const deletaUsuario = (id) => {[m
[32m+[m[32m        const ip = 'http://localhost:3001/deletausuario/'+id;[m
[32m+[m[32m        Axios.delete(ip).then((response) => {[m
[32m+[m[32m            setListaUsuarios([m
[32m+[m[32m                listaUsuarios.filter((val) => {[m
[32m+[m[32m                    return val.id != id[m
[32m+[m[32m                })[m
[32m+[m[32m            )[m
[32m+[m[32m        })[m
[32m+[m[32m    }[m
[32m+[m[32m    const getListaUsuarios = () =>{[m
[32m+[m[32m        Axios.get('http://localhost:3001/listausuarios').then((response) => {[m
[32m+[m[32m            setListaUsuarios(response.data);[m
[32m+[m[32m        })[m
[32m+[m[32m    }[m
[32m+[m[32m    return([m
[32m+[m[32m        <div>[m
[32m+[m[32m            <div>[m
[32m+[m[32m                <div className="section no-pad-bot">[m
[32m+[m[32m                        {getListaUsuarios()}[m
[32m+[m[32m                    <div className="container">[m
[32m+[m[32m                        <div className="row center">[m
[32m+[m[32m                            <table>[m
[32m+[m[32m                                <thead>[m
[32m+[m[32m                                <tr>[m
[32m+[m[32m                                    <th>Codigo</th>[m
[32m+[m[32m                                    <th>Nome</th>[m
[32m+[m[32m                                    <th>Senha</th>[m
[32m+[m[32m                                    <th>E-Mail</th>[m
[32m+[m[32m                                    <th>Excluir</th>[m
[32m+[m[32m                                </tr>[m
[32m+[m[32m                                </thead>[m
[32m+[m[32m                                <tbody>[m[41m    [m
[32m+[m[32m                                {listaUsuarios.map((val,key) => { return ([m
[32m+[m[32m                                <tr>[m[41m      [m
[32m+[m[32m                                    <td>{val.codigo}</td>[m
[32m+[m[32m                                    <td>{val.usuario}</td>[m
[32m+[m[32m                                    <td>{val.senha}</td>[m
[32m+[m[32m                                    <td>{val.email}</td>[m
[32m+[m[32m                                    <td><button onClick={() => {[m
[32m+[m[32m                                        deletaUsuario(val.codigo)[m
[32m+[m[32m                                    }}>Excluir</button> </td>[m
[32m+[m[32m                                </tr>[m
[32m+[m[32m                                )[m
[32m+[m[32m                                })}[m[41m  [m
[32m+[m[32m                                </tbody>[m
[32m+[m[32m                            </table>[m
[32m+[m[32m                        </div>[m
[32m+[m[32m                    </div>[m
[32m+[m[32m                </div>[m
[32m+[m[32m            </div>[m
[32m+[m[32m        </div>[m
[32m+[m[32m    );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default Usuarios;[m
