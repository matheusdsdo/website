[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./utils');[m
[32m+[m[32mvar bind = require('./helpers/bind');[m
[32m+[m[32mvar Axios = require('./core/Axios');[m
[32m+[m[32mvar mergeConfig = require('./core/mergeConfig');[m
[32m+[m[32mvar defaults = require('./defaults');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create an instance of Axios[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} defaultConfig The default config for the instance[m
[32m+[m[32m * @return {Axios} A new instance of Axios[m
[32m+[m[32m */[m
[32m+[m[32mfunction createInstance(defaultConfig) {[m
[32m+[m[32m  var context = new Axios(defaultConfig);[m
[32m+[m[32m  var instance = bind(Axios.prototype.request, context);[m
[32m+[m
[32m+[m[32m  // Copy axios.prototype to instance[m
[32m+[m[32m  utils.extend(instance, Axios.prototype, context);[m
[32m+[m
[32m+[m[32m  // Copy context to instance[m
[32m+[m[32m  utils.extend(instance, context);[m
[32m+[m
[32m+[m[32m  return instance;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Create the default instance to be exported[m
[32m+[m[32mvar axios = createInstance(defaults);[m
[32m+[m
[32m+[m[32m// Expose Axios class to allow class inheritance[m
[32m+[m[32maxios.Axios = Axios;[m
[32m+[m
[32m+[m[32m// Factory for creating new instances[m
[32m+[m[32maxios.create = function create(instanceConfig) {[m
[32m+[m[32m  return createInstance(mergeConfig(axios.defaults, instanceConfig));[m
[32m+[m[32m};[m
