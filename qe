[1mnew file mode 100644[m
[1mindex 0000000..79dfd09[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/index.js[m
[36m@@ -0,0 +1 @@[m
[32m+[m[32mmodule.exports = require('./lib/axios');[m
\ No newline at end of file[m
[1mdiff --git a/servidor/node_modules/axios/lib/adapters/README.md b/servidor/node_modules/axios/lib/adapters/README.md[m
[1mnew file mode 100644[m
[1mindex 0000000..68f1118[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/adapters/README.md[m
[36m@@ -0,0 +1,37 @@[m
[32m+[m[32m# axios // adapters[m
[32m+[m
[32m+[m[32mThe modules under `adapters/` are modules that handle dispatching a request and settling a returned `Promise` once a response is received.[m
[32m+[m
[32m+[m[32m## Example[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar settle = require('./../core/settle');[m
[32m+[m
[32m+[m[32mmodule.exports = function myAdapter(config) {[m
[32m+[m[32m  // At this point:[m
[32m+[m[32m  //  - config has been merged with defaults[m
[32m+[m[32m  //  - request transformers have already run[m
[32m+[m[32m  //  - request interceptors have already run[m
[32m+[m[41m  [m
[32m+[m[32m  // Make the request using config provided[m
[32m+[m[32m  // Upon response settle the Promise[m
[32m+[m
[32m+[m[32m  return new Promise(function(resolve, reject) {[m
[32m+[m[41m  [m
[32m+[m[32m    var response = {[m
[32m+[m[32m      data: responseData,[m
[32m+[m[32m      status: request.status,[m
[32m+[m[32m      statusText: request.statusText,[m
[32m+[m[32m      headers: responseHeaders,[m
[32m+[m[32m      config: config,[m
[32m+[m[32m      request: request[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    settle(resolve, reject, response);[m
[32m+[m
[32m+[m[32m    // From here:[m
[32m+[m[32m    //  - response transformers will run[m
[32m+[m[32m    //  - response interceptors will run[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m[32m```[m
[1mdiff --git a/servidor/node_modules/axios/lib/adapters/http.js b/servidor/node_modules/axios/lib/adapters/http.js[m
[1mnew file mode 100644[m
[1mindex 0000000..f32241f[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/adapters/http.js[m
[36m@@ -0,0 +1,303 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./../utils');[m
[32m+[m[32mvar settle = require('./../core/settle');[m
[32m+[m[32mvar buildFullPath = require('../core/buildFullPath');[m
[32m+[m[32mvar buildURL = require('./../helpers/buildURL');[m
[32m+[m[32mvar http = require('http');[m
[32m+[m[32mvar https = require('https');[m
[32m+[m[32mvar httpFollow = require('follow-redirects').http;[m
[32m+[m[32mvar httpsFollow = require('follow-redirects').https;[m
[32m+[m[32mvar url = require('url');[m
[32m+[m[32mvar zlib = require('zlib');[m
[32m+[m[32mvar pkg = require('./../../package.json');[m
[32m+[m[32mvar createError = require('../core/createError');[m
[32m+[m[32mvar enhanceError = require('../core/enhanceError');[m
[32m+[m
[32m+[m[32mvar isHttps = /https:?/;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @param {http.ClientRequestArgs} options[m
[32m+[m[32m * @param {AxiosProxyConfig} proxy[m
[32m+[m[32m * @param {string} location[m
[32m+[m[32m */[m
[32m+[m[32mfunction setProxy(options, proxy, location) {[m
[32m+[m[32m  options.hostname = proxy.host;[m
[32m+[m[32m  options.host = proxy.host;[m
[32m+[m[32m  options.port = proxy.port;[m
[32m+[m[32m  options.path = location;[m
[32m+[m
[32m+[m[32m  // Basic proxy authorization[m
[32m+[m[32m  if (proxy.auth) {[m
[32m+[m[32m    var base64 = Buffer.from(proxy.auth.username + ':' + proxy.auth.password, 'utf8').toString('base64');[m
[32m+[m[32m    options.headers['Proxy-Authorization'] = 'Basic ' + base64;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // If a proxy is used, any redirects must also pass through the proxy[m
[32m+[m[32m  options.beforeRedirect = function beforeRedirect(redirection) {[m
[32m+[m[32m    redirection.headers.host = redirection.host;[m
[32m+[m[32m    setProxy(redirection, proxy, redirection.href);[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*eslint consistent-return:0*/[m
[32m+[m[32mmodule.exports = function httpAdapter(config) {[m
[32m+[m[32m  return new Promise(function dispatchHttpRequest(resolvePromise, rejectPromise) {[m
[32m+[m[32m    var resolve = function resolve(value) {[m
[32m+[m[32m      resolvePromise(value);[m
[32m+[m[32m    };[m
[32m+[m[32m    var reject = function reject(value) {[m
[32m+[m[32m      rejectPromise(value);[m
[32m+[m[32m    };[m
[32m+[m[32m    var data = config.data;[m
[32m+[m[32m    var headers = config.headers;[m
[32m+[m
[32m+[m[32m    // Set User-Agent (required by some servers)[m
[32m+[m[32m    // Only set header if it hasn't been set in config[m
[32m+[m[32m    // See https://github.com/axios/axios/issues/69[m
[32m+[m[32m    if (!headers['User-Agent'] && !headers['user-agent']) {[m
[32m+[m[32m      headers['User-Agent'] = 'axios/' + pkg.version;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data && !utils.isStream(data)) {[m
[32m+[m[32m      if (Buffer.isBuffer(data)) {[m
[32m+[m[32m        // Nothing to do...[m
[32m+[m[32m      } else if (utils.isArrayBuffer(data)) {[m
[32m+[m[32m        data = Buffer.from(new Uint8Array(data));[m
[32m+[m[32m      } else if (utils.isString(data)) {[m
[32m+[m[32m        data = Buffer.from(data, 'utf-8');[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return reject(createError([m
[32m+[m[32m          'Data after transformation must be a string, an ArrayBuffer, a Buffer, or a Stream',[m
[32m+[m[32m          config[m
[32m+[m[32m        ));[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Add Content-Length header if data exists[m
[32m+[m[32m      headers['Content-Length'] = data.length;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // HTTP basic authentication[m
[32m+[m[32m    var auth = undefined;[m
[32m+[m[32m    if (config.auth) {[m
[32m+[m[32m      var username = config.auth.username || '';[m
[32m+[m[32m      var password = config.auth.password || '';[m
[32m+[m[32m      auth = username + ':' + password;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Parse url[m
[32m+[m[32m    var fullPath = buildFullPath(config.baseURL, config.url);[m
[32m+[m[32m    var parsed = url.parse(fullPath);[m
[32m+[m[32m    var protocol = parsed.protocol || 'http:';[m
[32m+[m
[32m+[m[32m    if (!auth && parsed.auth) {[m
[32m+[m[32m      var urlAuth = parsed.auth.split(':');[m
[32m+[m[32m      var urlUsername = urlAuth[0] || '';[m
[32m+[m[32m      var urlPassword = urlAuth[1] || '';[m
[32m+[m[32m      auth = urlUsername + ':' + urlPassword;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (auth) {[m
[32m+[m[32m      delete headers.Authorization;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var isHttpsRequest = isHttps.test(protocol);[m
[32m+[m[32m    var agent = isHttpsRequest ? config.httpsAgent : config.httpAgent;[m
[32m+[m
[32m+[m[32m    var options = {[m
[32m+[m[32m      path: buildURL(parsed.path, config.params, config.paramsSerializer).replace(/^\?/, ''),[m
[32m+[m[32m      method: config.method.toUpperCase(),[m
[32m+[m[32m      headers: headers,[m
[32m+[m[32m      agent: agent,[m
[32m+[m[32m      agents: { http: config.httpAgent, https: config.httpsAgent },[m
[32m+[m[32m      auth: auth[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if (config.socketPath) {[m
[32m+[m[32m      options.socketPath = config.socketPath;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      options.hostname = parsed.hostname;[m
[32m+[m[32m      options.port = parsed.port;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var proxy = config.proxy;[m
[32m+[m[32m    if (!proxy && proxy !== false) {[m
[32m+[m[32m      var proxyEnv = protocol.slice(0, -1) + '_proxy';[m
[32m+[m[32m      var proxyUrl = process.env[proxyEnv] || process.env[proxyEnv.toUpperCase()];[m
[32m+[m[32m      if (proxyUrl) {[m
[32m+[m[32m        var parsedProxyUrl = url.parse(proxyUrl);[m
[32m+[m[32m        var noProxyEnv = process.env.no_proxy || process.env.NO_PROXY;[m
[32m+[m[32m        var shouldProxy = true;[m
[32m+[m
[32m+[m[32m        if (noProxyEnv) {[m
[32m+[m[32m          var noProxy = noProxyEnv.split(',').map(function trim(s) {[m
[32m+[m[32m            return s.trim();[m
[32m+[m[32m          });[m
[32m+[m
[32m+[m[32m          shouldProxy = !noProxy.some(function proxyMatch(proxyElement) {[m
[32m+[m[32m            if (!proxyElement) {[m
[32m+[m[32m              return false;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (proxyElement === '*') {[m
[32m+[m[32m              return true;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (proxyElement[0] === '.' &&[m
[32m+[m[32m                parsed.hostname.substr(parsed.hostname.length - proxyElement.length) === proxyElement) {[m
[32m+[m[32m              return true;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            return parsed.hostname === proxyElement;[m
[32m+[m[32m          });[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (shouldProxy) {[m
[32m+[m[32m          proxy = {[m
[32m+[m[32m            host: parsedProxyUrl.hostname,[m
[32m+[m[32m            port: parsedProxyUrl.port,[m
[32m+[m[32m            protocol: parsedProxyUrl.protocol[m
[32m+[m[32m          };[m
[32m+[m
[32m+[m[32m          if (parsedProxyUrl.auth) {[m
[32m+[m[32m            var proxyUrlAuth = parsedProxyUrl.auth.split(':');[m
[32m+[m[32m            proxy.auth = {[m
[32m+[m[32m              username: proxyUrlAuth[0],[m
[32m+[m[32m              password: proxyUrlAuth[1][m
[32m+[m[32m            };[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (proxy) {[m
[32m+[m[32m      options.headers.host = parsed.hostname + (parsed.port ? ':' + parsed.port : '');[m
[32m+[m[32m      setProxy(options, proxy, protocol + '//' + parsed.hostname + (parsed.port ? ':' + parsed.port : '') + options.path);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var transport;[m
[32m+[m[32m    var isHttpsProxy = isHttpsRequest && (proxy ? isHttps.test(proxy.protocol) : true);[m
[32m+[m[32m    if (config.transport) {[m
[32m+[m[32m      transport = config.transport;[m
[32m+[m[32m    } else if (config.maxRedirects === 0) {[m
[32m+[m[32m      transport = isHttpsProxy ? https : http;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      if (config.maxRedirects) {[m
[32m+[m[32m        options.maxRedirects = config.maxRedirects;[m
[32m+[m[32m      }[m
[32m+[m[32m      transport = isHttpsProxy ? httpsFollow : httpFollow;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (config.maxBodyLength > -1) {[m
[32m+[m[32m      options.maxBodyLength = config.maxBodyLength;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Create the request[m
[32m+[m[32m    var req = transport.request(options, function handleResponse(res) {[m
[32m+[m[32m      if (req.aborted) return;[m
[32m+[m
[32m+[m[32m      // uncompress the response body transparently if required[m
[32m+[m[32m      var stream = res;[m
[32m+[m
[32m+[m[32m      // return the last request in case of redirects[m
[32m+[m[32m      var lastRequest = res.req || req;[m
[32m+[m
[32m+[m
[32m+[m[32m      // if no content, is HEAD request or decompress disabled we should not decompress[m
[32m+[m[32m      if (res.statusCode !== 204 && lastRequest.method !== 'HEAD' && config.decompress !== false) {[m
[32m+[m[32m        switch (res.headers['content-encoding']) {[m
[32m+[m[32m        /*eslint default-case:0*/[m
[32m+[m[32m        case 'gzip':[m
[32m+[m[32m        case 'compress':[m
[32m+[m[32m        case 'deflate':[m
[32m+[m[32m        // add the unzipper to the body stream processing pipeline[m
[32m+[m[32m          stream = stream.pipe(zlib.createUnzip());[m
[32m+[m
[32m+[m[32m          // remove the content-encoding in order to not confuse downstream operations[m
[32m+[m[32m          delete res.headers['content-encoding'];[m
[32m+[m[32m          break;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var response = {[m
[32m+[m[32m        status: res.statusCode,[m
[32m+[m[32m        statusText: res.statusMessage,[m
[32m+[m[32m        headers: res.headers,[m
[32m+[m[32m        config: config,[m
[32m+[m[32m        request: lastRequest[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      if (config.responseType === 'stream') {[m
[32m+[m[32m        response.data = stream;[m
[32m+[m[32m        settle(resolve, reject, response);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        var responseBuffer = [];[m
[32m+[m[32m        stream.on('data', function handleStreamData(chunk) {[m
[32m+[m[32m          responseBuffer.push(chunk);[m
[32m+[m
[32m+[m[32m          // make sure the content length is not over the maxContentLength if specified[m
[32m+[m[32m          if (config.maxContentLength > -1 && Buffer.concat(responseBuffer).length > config.maxContentLength) {[m
[32m+[m[32m            stream.destroy();[m
[32m+[m[32m            reject(createError('maxContentLength size of ' + config.maxContentLength + ' exceeded',[m
[32m+[m[32m              config, null, lastRequest));[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        stream.on('error', function handleStreamError(err) {[m
[32m+[m[32m          if (req.aborted) return;[m
[32m+[m[32m          reject(enhanceError(err, config, null, lastRequest));[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        stream.on('end', function handleStreamEnd() {[m
[32m+[m[32m          var responseData = Buffer.concat(responseBuffer);[m
[32m+[m[32m          if (config.responseType !== 'arraybuffer') {[m
[32m+[m[32m            responseData = responseData.toString(config.responseEncoding);[m
[32m+[m[32m            if (!config.responseEncoding || config.responseEncoding === 'utf8') {[m
[32m+[m[32m              responseData = utils.stripBOM(responseData);[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          response.data = responseData;[m
[32m+[m[32m          settle(resolve, reject, response);[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Handle errors[m
[32m+[m[32m    req.on('error', function handleRequestError(err) {[m
[32m+[m[32m      if (req.aborted && err.code !== 'ERR_FR_TOO_MANY_REDIRECTS') return;[m
[32m+[m[32m      reject(enhanceError(err, config, null, req));[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Handle request timeout[m
[32m+[m[32m    if (config.timeout) {[m
[32m+[m[32m      // Sometime, the response will be very slow, and does not respond, the connect event will be block by event loop system.[m
[32m+[m[32m      // And timer callback will be fired, and abort() will be invoked before connection, then get "socket hang up" and code ECONNRESET.[m
[32m+[m[32m      // At this time, if we have a large number of request, nodejs will hang up some socket on background. and the number will up and up.[m
[32m+[m[32m      // And then these socket which be hang up will devoring CPU little by little.[m
[32m+[m[32m      // ClientRequest.setTimeout will be fired on the specify milliseconds, and can make sure that abort() will be fired after connect.[m
[32m+[m[32m      req.setTimeout(config.timeout, function handleRequestTimeout() {[m
[32m+[m[32m        req.abort();[m
[32m+[m[32m        reject(createError('timeout of ' + config.timeout + 'ms exceeded', config, 'ECONNABORTED', req));[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (config.cancelToken) {[m
[32m+[m[32m      // Handle cancellation[m
[32m+[m[32m      config.cancelToken.promise.then(function onCanceled(cancel) {[m
[32m+[m[32m        if (req.aborted) return;[m
[32m+[m
[32m+[m[32m        req.abort();[m
[32m+[m[32m        reject(cancel);[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Send the request[m
[32m+[m[32m    if (utils.isStream(data)) {[m
[32m+[m[32m      data.on('error', function handleStreamError(err) {[m
[32m+[m[32m        reject(enhanceError(err, config, null, req));[m
[32m+[m[32m      }).pipe(req);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      req.end(data);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
