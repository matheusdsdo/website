[1m+++ b/servidor/node_modules/axios/lib/helpers/isURLSameOrigin.js[m
[36m@@ -0,0 +1,68 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./../utils');[m
[32m+[m
[32m+[m[32mmodule.exports = ([m
[32m+[m[32m  utils.isStandardBrowserEnv() ?[m
[32m+[m
[32m+[m[32m  // Standard browser envs have full support of the APIs needed to test[m
[32m+[m[32m  // whether the request URL is of the same origin as current location.[m
[32m+[m[32m    (function standardBrowserEnv() {[m
[32m+[m[32m      var msie = /(msie|trident)/i.test(navigator.userAgent);[m
[32m+[m[32m      var urlParsingNode = document.createElement('a');[m
[32m+[m[32m      var originURL;[m
[32m+[m
[32m+[m[32m      /**[m
[32m+[m[32m    * Parse a URL to discover it's components[m
[32m+[m[32m    *[m
[32m+[m[32m    * @param {String} url The URL to be parsed[m
[32m+[m[32m    * @returns {Object}[m
[32m+[m[32m    */[m
[32m+[m[32m      function resolveURL(url) {[m
[32m+[m[32m        var href = url;[m
[32m+[m
[32m+[m[32m        if (msie) {[m
[32m+[m[32m        // IE needs attribute set twice to normalize properties[m
[32m+[m[32m          urlParsingNode.setAttribute('href', href);[m
[32m+[m[32m          href = urlParsingNode.href;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        urlParsingNode.setAttribute('href', href);[m
[32m+[m
[32m+[m[32m        // urlParsingNode provides the UrlUtils interface - http://url.spec.whatwg.org/#urlutils[m
[32m+[m[32m        return {[m
[32m+[m[32m          href: urlParsingNode.href,[m
[32m+[m[32m          protocol: urlParsingNode.protocol ? urlParsingNode.protocol.replace(/:$/, '') : '',[m
[32m+[m[32m          host: urlParsingNode.host,[m
[32m+[m[32m          search: urlParsingNode.search ? urlParsingNode.search.replace(/^\?/, '') : '',[m
[32m+[m[32m          hash: urlParsingNode.hash ? urlParsingNode.hash.replace(/^#/, '') : '',[m
[32m+[m[32m          hostname: urlParsingNode.hostname,[m
[32m+[m[32m          port: urlParsingNode.port,[m
[32m+[m[32m          pathname: (urlParsingNode.pathname.charAt(0) === '/') ?[m
[32m+[m[32m            urlParsingNode.pathname :[m
[32m+[m[32m            '/' + urlParsingNode.pathname[m
[32m+[m[32m        };[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      originURL = resolveURL(window.location.href);[m
[32m+[m
[32m+[m[32m      /**[m
[32m+[m[32m    * Determine if a URL shares the same origin as the current location[m
[32m+[m[32m    *[m
[32m+[m[32m    * @param {String} requestURL The URL to test[m
[32m+[m[32m    * @returns {boolean} True if URL shares the same origin, otherwise false[m
[32m+[m[32m    */[m
[32m+[m[32m      return function isURLSameOrigin(requestURL) {[m
[32m+[m[32m        var parsed = (utils.isString(requestURL)) ? resolveURL(requestURL) : requestURL;[m
[32m+[m[32m        return (parsed.protocol === originURL.protocol &&[m
[32m+[m[32m            parsed.host === originURL.host);[m
[32m+[m[32m      };[m
[32m+[m[32m    })() :[m
[32m+[m
[32m+[m[32m  // Non standard browser envs (web workers, react-native) lack needed support.[m
[32m+[m[32m    (function nonStandardBrowserEnv() {[m
[32m+[m[32m      return function isURLSameOrigin() {[m
[32m+[m[32m        return true;[m
[32m+[m[32m      };[m
[32m+[m[32m    })()[m
[32m+[m[32m);[m
[1mdiff --git a/servidor/node_modules/axios/lib/helpers/normalizeHeaderName.js b/servidor/node_modules/axios/lib/helpers/normalizeHeaderName.js[m
[1mnew file mode 100644[m
[1mindex 0000000..738c9fe[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/helpers/normalizeHeaderName.js[m
[36m@@ -0,0 +1,12 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('../utils');[m
[32m+[m
[32m+[m[32mmodule.exports = function normalizeHeaderName(headers, normalizedName) {[m
[32m+[m[32m  utils.forEach(headers, function processHeader(value, name) {[m
[32m+[m[32m    if (name !== normalizedName && name.toUpperCase() === normalizedName.toUpperCase()) {[m
[32m+[m[32m      headers[normalizedName] = value;[m
[32m+[m[32m      delete headers[name];[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/lib/helpers/parseHeaders.js b/servidor/node_modules/axios/lib/helpers/parseHeaders.js[m
[1mnew file mode 100644[m
[1mindex 0000000..8af2cc7[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/helpers/parseHeaders.js[m
[36m@@ -0,0 +1,53 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./../utils');[m
[32m+[m
[32m+[m[32m// Headers whose duplicates are ignored by node[m
[32m+[m[32m// c.f. https://nodejs.org/api/http.html#http_message_headers[m
[32m+[m[32mvar ignoreDuplicateOf = [[m
[32m+[m[32m  'age', 'authorization', 'content-length', 'content-type', 'etag',[m
[32m+[m[32m  'expires', 'from', 'host', 'if-modified-since', 'if-unmodified-since',[m
[32m+[m[32m  'last-modified', 'location', 'max-forwards', 'proxy-authorization',[m
[32m+[m[32m  'referer', 'retry-after', 'user-agent'[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse headers into an object[m
[32m+[m[32m *[m
[32m+[m[32m * ```[m
[32m+[m[32m * Date: Wed, 27 Aug 2014 08:58:49 GMT[m
[32m+[m[32m * Content-Type: application/json[m
[32m+[m[32m * Connection: keep-alive[m
[32m+[m[32m * Transfer-Encoding: chunked[m
[32m+[m[32m * ```[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} headers Headers needing to be parsed[m
[32m+[m[32m * @returns {Object} Headers parsed into an object[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function parseHeaders(headers) {[m
[32m+[m[32m  var parsed = {};[m
[32m+[m[32m  var key;[m
[32m+[m[32m  var val;[m
[32m+[m[32m  var i;[m
[32m+[m
[32m+[m[32m  if (!headers) { return parsed; }[m
[32m+[m
[32m+[m[32m  utils.forEach(headers.split('\n'), function parser(line) {[m
[32m+[m[32m    i = line.indexOf(':');[m
[32m+[m[32m    key = utils.trim(line.substr(0, i)).toLowerCase();[m
[32m+[m[32m    val = utils.trim(line.substr(i + 1));[m
[32m+[m
[32m+[m[32m    if (key) {[m
[32m+[m[32m      if (parsed[key] && ignoreDuplicateOf.indexOf(key) >= 0) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m      if (key === 'set-cookie') {[m
[32m+[m[32m        parsed[key] = (parsed[key] ? parsed[key] : []).concat([val]);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        parsed[key] = parsed[key] ? parsed[key] + ', ' + val : val;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  return parsed;[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/lib/helpers/spread.js b/servidor/node_modules/axios/lib/helpers/spread.js[m
[1mnew file mode 100644[m
[1mindex 0000000..25e3cdd[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/helpers/spread.js[m
[36m@@ -0,0 +1,27 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Syntactic sugar for invoking a function and expanding an array for arguments.[m
[32m+[m[32m *[m
[32m+[m[32m * Common use case would be to use `Function.prototype.apply`.[m
[32m+[m[32m *[m
[32m+[m[32m *  ```js[m
[32m+[m[32m *  function f(x, y, z) {}[m
[32m+[m[32m *  var args = [1, 2, 3];[m
[32m+[m[32m *  f.apply(null, args);[m
[32m+[m[32m *  ```[m
[32m+[m[32m *[m
[32m+[m[32m * With `spread` this example can be re-written.[m
[32m+[m[32m *[m
[32m+[m[32m *  ```js[m
[32m+[m[32m *  spread(function(x, y, z) {})([1, 2, 3]);[m
[32m+[m[32m *  ```[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Function} callback[m
[32m+[m[32m * @returns {Function}[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function spread(callback) {[m
[32m+[m[32m  return function wrap(arr) {[m
[32m+[m[32m    return callback.apply(null, arr);[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
