[32m+[m	[32m  var val;[m
[32m+[m	[32m  var i;[m
[32m+[m[41m	[m
[32m+[m	[32m  if (!headers) { return parsed; }[m
[32m+[m[41m	[m
[32m+[m	[32m  utils.forEach(headers.split('\n'), function parser(line) {[m
[32m+[m	[32m    i = line.indexOf(':');[m
[32m+[m	[32m    key = utils.trim(line.substr(0, i)).toLowerCase();[m
[32m+[m	[32m    val = utils.trim(line.substr(i + 1));[m
[32m+[m[41m	[m
[32m+[m	[32m    if (key) {[m
[32m+[m	[32m      if (parsed[key] && ignoreDuplicateOf.indexOf(key) >= 0) {[m
[32m+[m	[32m        return;[m
[32m+[m	[32m      }[m
