[32m+[m[32mmodule.exports = require("./").http;[m
[1mdiff --git a/servidor/node_modules/follow-redirects/https.js b/servidor/node_modules/follow-redirects/https.js[m
[1mnew file mode 100644[m
[1mindex 0000000..d21c921[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/follow-redirects/https.js[m
[36m@@ -0,0 +1 @@[m
[32m+[m[32mmodule.exports = require("./").https;[m
[1mdiff --git a/servidor/node_modules/follow-redirects/index.js b/servidor/node_modules/follow-redirects/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..0bd3af9[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/follow-redirects/index.js[m
[36m@@ -0,0 +1,525 @@[m
[32m+[m[32mvar url = require("url");[m
[32m+[m[32mvar URL = url.URL;[m
[32m+[m[32mvar http = require("http");[m
[32m+[m[32mvar https = require("https");[m
[32m+[m[32mvar Writable = require("stream").Writable;[m
[32m+[m[32mvar assert = require("assert");[m
[32m+[m[32mvar debug = require("./debug");[m
[32m+[m
[32m+[m[32m// Create handlers that pass events from native requests[m
[32m+[m[32mvar eventHandlers = Object.create(null);[m
[32m+[m[32m["abort", "aborted", "connect", "error", "socket", "timeout"].forEach(function (event) {[m
[32m+[m[32m  eventHandlers[event] = function (arg1, arg2, arg3) {[m
[32m+[m[32m    this._redirectable.emit(event, arg1, arg2, arg3);[m
[32m+[m[32m  };[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Error types with codes[m
[32m+[m[32mvar RedirectionError = createErrorType([m
[32m+[m[32m  "ERR_FR_REDIRECTION_FAILURE",[m
[32m+[m[32m  ""[m
[32m+[m[32m);[m
[32m+[m[32mvar TooManyRedirectsError = createErrorType([m
[32m+[m[32m  "ERR_FR_TOO_MANY_REDIRECTS",[m
[32m+[m[32m  "Maximum number of redirects exceeded"[m
[32m+[m[32m);[m
[32m+[m[32mvar MaxBodyLengthExceededError = createErrorType([m
[32m+[m[32m  "ERR_FR_MAX_BODY_LENGTH_EXCEEDED",[m
[32m+[m[32m  "Request body larger than maxBodyLength limit"[m
[32m+[m[32m);[m
[32m+[m[32mvar WriteAfterEndError = createErrorType([m
[32m+[m[32m  "ERR_STREAM_WRITE_AFTER_END",[m
[32m+[m[32m  "write after end"[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32m// An HTTP(S) request that can be redirected[m
[32m+[m[32mfunction RedirectableRequest(options, responseCallback) {[m
[32m+[m[32m  // Initialize the request[m
[32m+[m[32m  Writable.call(this);[m
[32m+[m[32m  this._sanitizeOptions(options);[m
[32m+[m[32m  this._options = options;[m
[32m+[m[32m  this._ended = false;[m
[32m+[m[32m  this._ending = false;[m
[32m+[m[32m  this._redirectCount = 0;[m
[32m+[m[32m  this._redirects = [];[m
[32m+[m[32m  this._requestBodyLength = 0;[m
[32m+[m[32m  this._requestBodyBuffers = [];[m
[32m+[m
[32m+[m[32m  // Attach a callback if passed[m
[32m+[m[32m  if (responseCallback) {[m
[32m+[m[32m    this.on("response", responseCallback);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // React to responses of native requests[m
[32m+[m[32m  var self = this;[m
[32m+[m[32m  this._onNativeResponse = function (response) {[m
[32m+[m[32m    self._processResponse(response);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Perform the first request[m
[32m+[m[32m  this._performRequest();[m
[32m+[m[32m}[m
[32m+[m[32mRedirectableRequest.prototype = Object.create(Writable.prototype);[m
[32m+[m
[32m+[m[32mRedirectableRequest.prototype.abort = function () {[m
[32m+[m[32m  // Abort the internal request[m
[32m+[m[32m  this._currentRequest.removeAllListeners();[m
[32m+[m[32m  this._currentRequest.on("error", noop);[m
[32m+[m[32m  this._currentRequest.abort();[m
[32m+[m
[32m+[m[32m  // Abort this request[m
[32m+[m[32m  this.emit("abort");[m
[32m+[m[32m  this.removeAllListeners();[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Writes buffered data to the current native request[m
[32m+[m[32mRedirectableRequest.prototype.write = function (data, encoding, callback) {[m
[32m+[m[32m  // Writing is not allowed if end has been called[m
[32m+[m[32m  if (this._ending) {[m
[32m+[m[32m    throw new WriteAfterEndError();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Validate input and shift parameters if necessary[m
[32m+[m[32m  if (!(typeof data === "string" || typeof data === "object" && ("length" in data))) {[m
[32m+[m[32m    throw new TypeError("data should be a string, Buffer or Uint8Array");[m
[32m+[m[32m  }[m
[32m+[m[32m  if (typeof encoding === "function") {[m
[32m+[m[32m    callback = encoding;[m
[32m+[m[32m    encoding = null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Ignore empty buffers, since writing them doesn't invoke the callback[m
[32m+[m[32m  // https://github.com/nodejs/node/issues/22066[m
[32m+[m[32m  if (data.length === 0) {[m
[32m+[m[32m    if (callback) {[m
[32m+[m[32m      callback();[m
[32m+[m[32m    }[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m[32m  // Only write when we don't exceed the maximum body length[m
[32m+[m[32m  if (this._requestBodyLength + data.length <= this._options.maxBodyLength) {[m
[32m+[m[32m    this._requestBodyLength += data.length;[m
[32m+[m[32m    this._requestBodyBuffers.push({ data: data, encoding: encoding });[m
[32m+[m[32m    this._currentRequest.write(data, encoding, callback);[m
[32m+[m[32m  }[m
[32m+[m[32m  // Error when we exceed the maximum body length[m
[32m+[m[32m  else {[m
[32m+[m[32m    this.emit("error", new MaxBodyLengthExceededError());[m
[32m+[m[32m    this.abort();[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Ends the current native request[m
[32m+[m[32mRedirectableRequest.prototype.end = function (data, encoding, callback) {[m
[32m+[m[32m  // Shift parameters if necessary[m
[32m+[m[32m  if (typeof data === "function") {[m
[32m+[m[32m    callback = data;[m
[32m+[m[32m    data = encoding = null;[m
[32m+[m[32m  }[m
[32m+[m[32m  else if (typeof encoding === "function") {[m
[32m+[m[32m    callback = encoding;[m
[32m+[m[32m    encoding = null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Write data if needed and end[m
[32m+[m[32m  if (!data) {[m
[32m+[m[32m    this._ended = this._ending = true;[m
[32m+[m[32m    this._currentRequest.end(null, null, callback);[m
[32m+[m[32m  }[m
[32m+[m[32m  else {[m
[32m+[m[32m    var self = this;[m
[32m+[m[32m    var currentRequest = this._currentRequest;[m
[32m+[m[32m    this.write(data, encoding, function () {[m
[32m+[m[32m      self._ended = true;[m
[32m+[m[32m      currentRequest.end(null, null, callback);[m
[32m+[m[32m    });[m
[32m+[m[32m    this._ending = true;[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Sets a header value on the current native request[m
[32m+[m[32mRedirectableRequest.prototype.setHeader = function (name, value) {[m
[32m+[m[32m  this._options.headers[name] = value;[m
[32m+[m[32m  this._currentRequest.setHeader(name, value);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Clears a header value on the current native request[m
[32m+[m[32mRedirectableRequest.prototype.removeHeader = function (name) {[m
[32m+[m[32m  delete this._options.headers[name];[m
[32m+[m[32m  this._currentRequest.removeHeader(name);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Global timeout for all underlying requests[m
[32m+[m[32mRedirectableRequest.prototype.setTimeout = function (msecs, callback) {[m
[32m+[m[32m  var self = this;[m
[32m+[m[32m  if (callback) {[m
[32m+[m[32m    this.on("timeout", callback);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Sets up a timer to trigger a timeout event[m
[32m+[m[32m  function startTimer() {[m
[32m+[m[32m    if (self._timeout) {[m
[32m+[m[32m      clearTimeout(self._timeout);[m
[32m+[m[32m    }[m
[32m+[m[32m    self._timeout = setTimeout(function () {[m
[32m+[m[32m      self.emit("timeout");[m
[32m+[m[32m      clearTimer();[m
[32m+[m[32m    }, msecs);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Prevent a timeout from triggering[m
[32m+[m[32m  function clearTimer() {[m
[32m+[m[32m    clearTimeout(this._timeout);[m
[32m+[m[32m    if (callback) {[m
[32m+[m[32m      self.removeListener("timeout", callback);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (!this.socket) {[m
[32m+[m[32m      self._currentRequest.removeListener("socket", startTimer);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Start the timer when the socket is opened[m
[32m+[m[32m  if (this.socket) {[m
[32m+[m[32m    startTimer();[m
[32m+[m[32m  }[m
[32m+[m[32m  else {[m
[32m+[m[32m    this._currentRequest.once("socket", startTimer);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  this.once("response", clearTimer);[m
[32m+[m[32m  this.once("error", clearTimer);[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Proxy all other public ClientRequest methods[m
[32m+[m[32m[[m
[32m+[m[32m  "flushHeaders", "getHeader",[m
[32m+[m[32m  "setNoDelay", "setSocketKeepAlive",[m
[32m+[m[32m].forEach(function (method) {[m
[32m+[m[32m  RedirectableRequest.prototype[method] = function (a, b) {[m
[32m+[m[32m    return this._currentRequest[method](a, b);[m
[32m+[m[32m  };[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Proxy all public ClientRequest properties[m
[32m+[m[32m["aborted", "connection", "socket"].forEach(function (property) {[m
[32m+[m[32m  Object.defineProperty(RedirectableRequest.prototype, property, {[m
[32m+[m[32m    get: function () { return this._currentRequest[property]; },[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mRedirectableRequest.prototype._sanitizeOptions = function (options) {[m
[32m+[m[32m  // Ensure headers are always present[m
[32m+[m[32m  if (!options.headers) {[m
[32m+[m[32m    options.headers = {};[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Since http.request treats host as an alias of hostname,[m
[32m+[m[32m  // but the url module interprets host as hostname plus port,[m
[32m+[m[32m  // eliminate the host property to avoid confusion.[m
[32m+[m[32m  if (options.host) {[m
[32m+[m[32m    // Use hostname if set, because it has precedence[m
[32m+[m[32m    if (!options.hostname) {[m
[32m+[m[32m      options.hostname = options.host;[m
[32m+[m[32m    }[m
[32m+[m[32m    delete options.host;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Complete the URL object when necessary[m
[32m+[m[32m  if (!options.pathname && options.path) {[m
[32m+[m[32m    var searchPos = options.path.indexOf("?");[m
[32m+[m[32m    if (searchPos < 0) {[m
[32m+[m[32m      options.pathname = options.path;[m
[32m+[m[32m    }[m
[32m+[m[32m    else {[m
[32m+[m[32m      options.pathname = options.path.substring(0, searchPos);[m
[32m+[m[32m      options.search = options.path.substring(searchPos);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32m// Executes the next native request (initial or redirect)[m
[32m+[m[32mRedirectableRequest.prototype._performRequest = function () {[m
[32m+[m[32m  // Load the native protocol[m
[32m+[m[32m  var protocol = this._options.protocol;[m
[32m+[m[32m  var nativeProtocol = this._options.nativeProtocols[protocol];[m
[32m+[m[32m  if (!nativeProtocol) {[m
[32m+[m[32m    this.emit("error", new TypeError("Unsupported protocol " + protocol));[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // If specified, use the agent corresponding to the protocol[m
[32m+[m[32m  // (HTTP and HTTPS use different types of agents)[m
[32m+[m[32m  if (this._options.agents) {[m
[32m+[m[32m    var scheme = protocol.substr(0, protocol.length - 1);[m
[32m+[m[32m    this._options.agent = this._options.agents[scheme];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Create the native request[m
[32m+[m[32m  var request = this._currentRequest =[m
[32m+[m[32m        nativeProtocol.request(this._options, this._onNativeResponse);[m
[32m+[m[32m  this._currentUrl = url.format(this._options);[m
[32m+[m
[32m+[m[32m  // Set up event handlers[m
[32m+[m[32m  request._redirectable = this;[m
[32m+[m[32m  for (var event in eventHandlers) {[m
[32m+[m[32m    /* istanbul ignore else */[m
[32m+[m[32m    if (event) {[m
[32m+[m[32m      request.on(event, eventHandlers[event]);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // End a redirected request[m
[32m+[m[32m  // (The first request must be ended explicitly with RedirectableRequest#end)[m
[32m+[m[32m  if (this._isRedirect) {[m
[32m+[m[32m    // Write the request entity and end.[m
[32m+[m[32m    var i = 0;[m
[32m+[m[32m    var self = this;[m
[32m+[m[32m    var buffers = this._requestBodyBuffers;[m
[32m+[m[32m    (function writeNext(error) {[m
[32m+[m[32m      // Only write if this request has not been redirected yet[m
[32m+[m[32m      /* istanbul ignore else */[m
[32m+[m[32m      if (request === self._currentRequest) {[m
[32m+[m[32m        // Report any write errors[m
[32m+[m[32m        /* istanbul ignore if */[m
[32m+[m[32m        if (error) {[m
[32m+[m[32m          self.emit("error", error);[m
[32m+[m[32m        }[m
[32m+[m[32m        // Write the next buffer if there are still left[m
[32m+[m[32m        else if (i < buffers.length) {[m
[32m+[m[32m          var buffer = buffers[i++];[m
[32m+[m[32m          /* istanbul ignore else */[m
[32m+[m[32m          if (!request.finished) {[m
[32m+[m[32m            request.write(buffer.data, buffer.encoding, writeNext);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        // End the request if `end` has been called on us[m
[32m+[m[32m        else if (self._ended) {[m
[32m+[m[32m          request.end();[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }());[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Processes a response from the current native request[m
[32m+[m[32mRedirectableRequest.prototype._processResponse = function (response) {[m
[32m+[m[32m  // Store the redirected response[m
[32m+[m[32m  var statusCode = response.statusCode;[m
[32m+[m[32m  if (this._options.trackRedirects) {[m
[32m+[m[32m    this._redirects.push({[m
[32m+[m[32m      url: this._currentUrl,[m
[32m+[m[32m      headers: response.headers,[m
[32m+[m[32m      statusCode: statusCode,[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // RFC7231Â§6.4: The 3xx (Redirection) class of status code indicates[m
[32m+[m[32m  // that further action needs to be taken by the user agent in order to[m
[32m+[m[32m  // fulfill the request. If a Location header field is provided,[m
[32m+[m[32m  // the user agent MAY automatically redirect its request to the URI[m
[32m+[m[32m  // referenced by the Location field value,[m
[32m+[m[32m  // even if the specific status code is not understood.[m
[32m+[m[32m  var location = response.headers.location;[m
[32m+[m[32m  if (location && this._options.followRedirects !== false &&[m
[32m+[m[32m      statusCode >= 300 && statusCode < 400) {[m
[32m+[m[32m    // Abort the current request[m
[32m+[m[32m    this._currentRequest.removeAllListeners();[m
[32m+[m[32m    this._currentRequest.on("error", noop);[m
[32m+[m[32m    this._currentRequest.abort();[m
[32m+[m[32m    // Discard the remainder of the response to avoid waiting for data[m
[32m+[m[32m    response.destroy();[m
[32m+[m
[32m+[m[32m    // RFC7231Â§6.4: A client SHOULD detect and intervene[m
[32m+[m[32m    // in cyclical redirections (i.e., "infinite" redirection loops).[m
[32m+[m[32m    if (++this._redirectCount > this._options.maxRedirects) {[m
[32m+[m[32m      this.emit("error", new TooManyRedirectsError());[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // RFC7231Â§6.4: Automatic redirection needs to done with[m
[32m+[m[32m    // care for methods not known to be safe, [â¦][m
[32m+[m[32m    // RFC7231Â§6.4.2â3: For historical reasons, a user agent MAY change[m
[32m+[m[32m    // the request method from POST to GET for the subsequent request.[m
[32m+[m[32m    if ((statusCode === 301 || statusCode === 302) && this._options.method === "POST" ||[m
[32m+[m[32m        // RFC7231Â§6.4.4: The 303 (See Other) status code indicates that[m
[32m+[m[32m        // the server is redirecting the user agent to a different resource [â¦][m
[32m+[m[32m        // A user agent can perform a retrieval request targeting that URI[m
[32m+[m[32m        // (a GET or HEAD request if using HTTP) [â¦][m
[32m+[m[32m        (statusCode === 303) && !/^(?:GET|HEAD)$/.test(this._options.method)) {[m
[32m+[m[32m      this._options.method = "GET";[m
[32m+[m[32m      // Drop a possible entity and headers related to it[m
[32m+[m[32m      this._requestBodyBuffers = [];[m
[32m+[m[32m      removeMatchingHeaders(/^content-/i, this._options.headers);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Drop the Host header, as the redirect might lead to a different host[m
[32m+[m[32m    var previousHostName = removeMatchingHeaders(/^host$/i, this._options.headers) ||[m
[32m+[m[32m      url.parse(this._currentUrl).hostname;[m
[32m+[m
[32m+[m[32m    // Create the redirected request[m
[32m+[m[32m    var redirectUrl = url.resolve(this._currentUrl, location);[m
[32m+[m[32m    debug("redirecting to", redirectUrl);[m
[32m+[m[32m    this._isRedirect = true;[m
[32m+[m[32m    var redirectUrlParts = url.parse(redirectUrl);[m
[32m+[m[32m    Object.assign(this._options, redirectUrlParts);[m
[32m+[m
[32m+[m[32m    // Drop the Authorization header if redirecting to another host[m
[32m+[m[32m    if (redirectUrlParts.hostname !== previousHostName) {[m
[32m+[m[32m      removeMatchingHeaders(/^authorization$/i, this._options.headers);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Evaluate the beforeRedirect callback[m
[32m+[m[32m    if (typeof this._options.beforeRedirect === "function") {[m
[32m+[m[32m      var responseDetails = { headers: response.headers };[m
[32m+[m[32m      try {[m
[32m+[m[32m        this._options.beforeRedirect.call(null, this._options, responseDetails);[m
[32m+[m[32m      }[m
[32m+[m[32m      catch (err) {[m
[32m+[m[32m        this.emit("error", err);[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m      this._sanitizeOptions(this._options);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Perform the redirected request[m
[32m+[m[32m    try {[m
[32m+[m[32m      this._performRequest();[m
[32m+[m[32m    }[m
[32m+[m[32m    catch (cause) {[m
[32m+[m[32m      var error = new RedirectionError("Redirected request failed: " + cause.message);[m
[32m+[m[32m      error.cause = cause;[m
[32m+[m[32m      this.emit("error", error);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  else {[m
[32m+[m[32m    // The response is not a redirect; return it as-is[m
[32m+[m[32m    response.responseUrl = this._currentUrl;[m
[32m+[m[32m    response.redirects = this._redirects;[m
[32m+[m[32m    this.emit("response", response);[m
[32m+[m
[32m+[m[32m    // Clean up[m
[32m+[m[32m    this._requestBodyBuffers = [];[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Wraps the key/value object of protocols with redirect functionality[m
[32m+[m[32mfunction wrap(protocols) {[m
[32m+[m[32m  // Default settings[m
[32m+[m[32m  var exports = {[m
[32m+[m[32m    maxRedirects: 21,[m
[32m+[m[32m    maxBodyLength: 10 * 1024 * 1024,[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Wrap each protocol[m
[32m+[m[32m  var nativeProtocols = {};[m
[32m+[m[32m  Object.keys(protocols).forEach(function (scheme) {[m
[32m+[m[32m    var protocol = scheme + ":";[m
[32m+[m[32m    var nativeProtocol = nativeProtocols[protocol] = protocols[scheme];[m
[32m+[m[32m    var wrappedProtocol = exports[scheme] = Object.create(nativeProtocol);[m
[32m+[m
[32m+[m[32m    // Executes a request, following redirects[m
[32m+[m[32m    function request(input, options, callback) {[m
[32m+[m[32m      // Parse parameters[m
[32m+[m[32m      if (typeof input === "string") {[m
[32m+[m[32m        var urlStr = input;[m
[32m+[m[32m        try {[m
[32m+[m[32m          input = urlToOptions(new URL(urlStr));[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (err) {[m
[32m+[m[32m          /* istanbul ignore next */[m
[32m+[m[32m          input = url.parse(urlStr);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      else if (URL && (input instanceof URL)) {[m
[32m+[m[32m        input = urlToOptions(input);[m
[32m+[m[32m      }[m
[32m+[m[32m      else {[m
[32m+[m[32m        callback = options;[m
[32m+[m[32m        options = input;[m
[32m+[m[32m        input = { protocol: protocol };[m
[32m+[m[32m      }[m
[32m+[m[32m      if (typeof options === "function") {[m
[32m+[m[32m        callback = options;[m
[32m+[m[32m        options = null;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Set defaults[m
[32m+[m[32m      options = Object.assign({[m
[32m+[m[32m        maxRedirects: exports.maxRedirects,[m
[32m+[m[32m        maxBodyLength: exports.maxBodyLength,[m
[32m+[m[32m      }, input, options);[m
[32m+[m[32m      options.nativeProtocols = nativeProtocols;[m
[32m+[m
[32m+[m[32m      assert.equal(options.protocol, protocol, "protocol mismatch");[m
[32m+[m[32m      debug("options", options);[m
[32m+[m[32m      return new RedirectableRequest(options, callback);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Executes a GET request, following redirects[m
[32m+[m[32m    function get(input, options, callback) {[m
[32m+[m[32m      var wrappedRequest = wrappedProtocol.request(input, options, callback);[m
[32m+[m[32m      wrappedRequest.end();[m
[32m+[m[32m      return wrappedRequest;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Expose the properties on the wrapped protocol[m
[32m+[m[32m    Object.defineProperties(wrappedProtocol, {[m
[32m+[m[32m      request: { value: request, configurable: true, enumerable: true, writable: true },[m
[32m+[m[32m      get: { value: get, configurable: true, enumerable: true, writable: true },[m
[32m+[m[32m    });[m
[32m+[m[32m  });[m
[32m+[m[32m  return exports;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* istanbul ignore next */[m
[32m+[m[32mfunction noop() { /* empty */ }[m
[32m+[m
[32m+[m[32m// from https://github.com/nodejs/node/blob/master/lib/internal/url.js[m
[32m+[m[32mfunction urlToOptions(urlObject) {[m
[32m+[m[32m  var options = {[m
[32m+[m[32m    protocol: urlObject.protocol,[m
[32m+[m[32m    hostname: urlObject.hostname.startsWith("[") ?[m
[32m+[m[32m      /* istanbul ignore next */[m
[32m+[m[32m      urlObject.hostname.slice(1, -1) :[m
[32m+[m[32m      urlObject.hostname,[m
[32m+[m[32m    hash: urlObject.hash,[m
[32m+[m[32m    search: urlObject.search,[m
[32m+[m[32m    pathname: urlObject.pathname,[m
[32m+[m[32m    path: urlObject.pathname + urlObject.search,[m
[32m+[m[32m    href: urlObject.href,[m
[32m+[m[32m  };[m
[32m+[m[32m  if (urlObject.port !== "") {[m
[32m+[m[32m    options.port = Number(urlObject.port);[m
[32m+[m[32m  }[m
[32m+[m[32m  return options;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction removeMatchingHeaders(regex, headers) {[m
[32m+[m[32m  var lastValue;[m
[32m+[m[32m  for (var header in headers) {[m
[32m+[m[32m    if (regex.test(header)) {[m
[32m+[m[32m      lastValue = headers[header];[m
[32m+[m[32m      delete headers[header];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return lastValue;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction createErrorType(code, defaultMessage) {[m
[32m+[m[32m  function CustomError(message) {[m
[32m+[m[32m    Error.captureStackTrace(this, this.constructor);[m
[32m+[m[32m    this.message = message || defaultMessage;[m
[32m+[m[32m  }[m
[32m+[m[32m  CustomError.prototype = new Error();[m
[32m+[m[32m  CustomError.prototype.constructor = CustomError;[m
[32m+[m[32m  CustomError.prototype.name = "Error [" + code + "]";[m
[32m+[m[32m  CustomError.prototype.code = code;[m
[32m+[m[32m  return CustomError;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Exports[m
[32m+[m[32mmodule.exports = wrap({ http: http, https: https });[m
[32m+[m[32mmodule.exports.wrap = wrap;[m
