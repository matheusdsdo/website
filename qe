[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/*eslint no-console:0*/[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Supply a warning to the developer that a method they are using[m
[32m+[m[32m * has been deprecated.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} method The name of the deprecated method[m
[32m+[m[32m * @param {string} [instead] The alternate method to use if applicable[m
[32m+[m[32m * @param {string} [docs] The documentation URL to get further details[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function deprecatedMethod(method, instead, docs) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    console.warn([m
[32m+[m[32m      'DEPRECATED method `' + method + '`.' +[m
[32m+[m[32m      (instead ? ' Use `' + instead + '` instead.' : '') +[m
[32m+[m[32m      ' This method will be removed in a future release.');[m
[32m+[m
[32m+[m[32m    if (docs) {[m
[32m+[m[32m      console.warn('For more information about usage see ' + docs);[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (e) { /* Ignore */ }[m
[32m+[m[32m};[m
[1mdiff --git a/servidor/node_modules/axios/lib/helpers/isAbsoluteURL.js b/servidor/node_modules/axios/lib/helpers/isAbsoluteURL.js[m
[1mnew file mode 100644[m
[1mindex 0000000..d33e992[m
