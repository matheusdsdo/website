[32m+[m[32m/* 11 */[m
[32m+[m[32m/***/ (function(module, exports, __webpack_require__) {[m
[32m+[m
[32m+[m	[32m'use strict';[m
[32m+[m[41m	[m
[32m+[m	[32mvar utils = __webpack_require__(2);[m
[32m+[m[41m	[m
[32m+[m	[32mmodule.exports = function normalizeHeaderName(headers, normalizedName) {[m
[32m+[m	[32m  utils.forEach(headers, function processHeader(value, name) {[m
[32m+[m	[32m    if (name !== normalizedName && name.toUpperCase() === normalizedName.toUpperCase()) {[m
