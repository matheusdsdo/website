[32m+[m	[32m  function getMergedValue(target, source) {[m
[32m+[m	[32m    if (utils.isPlainObject(target) && utils.isPlainObject(source)) {[m
[32m+[m	[32m      return utils.merge(target, source);[m
[32m+[m	[32m    } else if (utils.isPlainObject(source)) {[m
[32m+[m	[32m      return utils.merge({}, source);[m
[32m+[m	[32m    } else if (utils.isArray(source)) {[m
[32m+[m	[32m      return source.slice();[m
[32m+[m	[32m    }[m
[32m+[m	[32m    return source;[m
[32m+[m	[32m  }[m
[32m+[m[41m	[m
[32m+[m	[32m  function mergeDeepProperties(prop) {[m
[32m+[m	[32m    if (!utils.isUndefined(config2[prop])) {[m
[32m+[m	[32m      config[prop] = getMergedValue(config1[prop], config2[prop]);[m
[32m+[m	[32m    } else if (!utils.isUndefined(config1[prop])) {[m
[32m+[m	[32m      config[prop] = getMergedValue(undefined, config1[prop]);[m
[32m+[m	[32m    }[m
[32m+[m	[32m  }[m
