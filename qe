[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./utils');[m
[32m+[m[32mvar normalizeHeaderName = require('./helpers/normalizeHeaderName');[m
[32m+[m
[32m+[m[32mvar DEFAULT_CONTENT_TYPE = {[m
[32m+[m[32m  'Content-Type': 'application/x-www-form-urlencoded'[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction setContentTypeIfUnset(headers, value) {[m
[32m+[m[32m  if (!utils.isUndefined(headers) && utils.isUndefined(headers['Content-Type'])) {[m
[32m+[m[32m    headers['Content-Type'] = value;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getDefaultAdapter() {[m
[32m+[m[32m  var adapter;[m
[32m+[m[32m  if (typeof XMLHttpRequest !== 'undefined') {[m
[32m+[m[32m    // For browsers use XHR adapter[m
[32m+[m[32m    adapter = require('./adapters/xhr');[m
[32m+[m[32m  } else if (typeof process !== 'undefined' && Object.prototype.toString.call(process) === '[object process]') {[m
[32m+[m[32m    // For node use HTTP adapter[m
[32m+[m[32m    adapter = require('./adapters/http');[m
[32m+[m[32m  }[m
[32m+[m[32m  return adapter;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar defaults = {[m
[32m+[m[32m  adapter: getDefaultAdapter(),[m
[32m+[m
[32m+[m[32m  transformRequest: [function transformRequest(data, headers) {[m
[32m+[m[32m    normalizeHeaderName(headers, 'Accept');[m
[32m+[m[32m    normalizeHeaderName(headers, 'Content-Type');[m
[32m+[m[32m    if (utils.isFormData(data) ||[m
[32m+[m[32m      utils.isArrayBuffer(data) ||[m
[32m+[m[32m      utils.isBuffer(data) ||[m
[32m+[m[32m      utils.isStream(data) ||[m
[32m+[m[32m      utils.isFile(data) ||[m
[32m+[m[32m      utils.isBlob(data)[m
[32m+[m[32m    ) {[m
[32m+[m[32m      return data;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (utils.isArrayBufferView(data)) {[m
[32m+[m[32m      return data.buffer;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (utils.isURLSearchParams(data)) {[m
[32m+[m[32m      setContentTypeIfUnset(headers, 'application/x-www-form-urlencoded;charset=utf-8');[m
[32m+[m[32m      return data.toString();[m
[32m+[m[32m    }[m
[32m+[m[32m    if (utils.isObject(data)) {[m
[32m+[m[32m      setContentTypeIfUnset(headers, 'application/json;charset=utf-8');[m
[32m+[m[32m      return JSON.stringify(data);[m
[32m+[m[32m    }[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }],[m
[32m+[m
[32m+[m[32m  transformResponse: [function transformResponse(data) {[m
[32m+[m[32m    /*eslint no-param-reassign:0*/[m
[32m+[m[32m    if (typeof data === 'string') {[m
[32m+[m[32m      try {[m
[32m+[m[32m        data = JSON.parse(data);[m
[32m+[m[32m      } catch (e) { /* Ignore */ }[m
[32m+[m[32m    }[m
[32m+[m[32m    return data;[m
[32m+[m[32m  }],[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * A timeout in milliseconds to abort a request. If set to 0 (default) a[m
[32m+[m[32m   * timeout is not created.[m
[32m+[m[32m   */[m
[32m+[m[32m  timeout: 0,[m
[32m+[m
[32m+[m[32m  xsrfCookieName: 'XSRF-TOKEN',[m
[32m+[m[32m  xsrfHeaderName: 'X-XSRF-TOKEN',[m
[32m+[m
[32m+[m[32m  maxContentLength: -1,[m
[32m+[m[32m  maxBodyLength: -1,[m
[32m+[m
[32m+[m[32m  validateStatus: function validateStatus(status) {[m
[32m+[m[32m    return status >= 200 && status < 300;[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
