[32m+[m[32m      /* eslint global-require: off */[m
[32m+[m[32m      debug = require("debug")("follow-redirects");[m
[32m+[m[32m    }[m
[32m+[m[32m    catch (error) {[m
[32m+[m[32m      debug = function () { /* */ };[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  debug.apply(null, arguments);[m
