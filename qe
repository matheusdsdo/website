[32m+[m	[32m    request.timeout = config.timeout;[m
[32m+[m[41m	[m
[32m+[m	[32m    // Listen for ready state[m
[32m+[m	[32m    request.onreadystatechange = function handleLoad() {[m
[32m+[m	[32m      if (!request || request.readyState !== 4) {[m
[32m+[m	[32m        return;[m
[32m+[m	[32m      }[m
[32m+[m[41m	[m
