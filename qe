[32m+[m[32maxios.Cancel = require('./cancel/Cancel');[m
[32m+[m[32maxios.CancelToken = require('./cancel/CancelToken');[m
[32m+[m[32maxios.isCancel = require('./cancel/isCancel');[m
[32m+[m
[32m+[m[32m// Expose all/spread[m
[32m+[m[32maxios.all = function all(promises) {[m
[32m+[m[32m  return Promise.all(promises);[m
[32m+[m[32m};[m
[32m+[m[32maxios.spread = require('./helpers/spread');[m
[32m+[m
[32m+[m[32m// Expose isAxiosError[m
[32m+[m[32maxios.isAxiosError = require('./helpers/isAxiosError');[m
[32m+[m
[32m+[m[32mmodule.exports = axios;[m
