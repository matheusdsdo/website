[1mnew file mode 100644[m
[1mindex 0000000..50d667b[m
[1m--- /dev/null[m
[1m+++ b/servidor/node_modules/axios/lib/core/InterceptorManager.js[m
[36m@@ -0,0 +1,52 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./../utils');[m
[32m+[m
[32m+[m[32mfunction InterceptorManager() {[m
[32m+[m[32m  this.handlers = [];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Add a new interceptor to the stack[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Function} fulfilled The function to handle `then` for a `Promise`[m
[32m+[m[32m * @param {Function} rejected The function to handle `reject` for a `Promise`[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Number} An ID used to remove interceptor later[m
[32m+[m[32m */[m
[32m+[m[32mInterceptorManager.prototype.use = function use(fulfilled, rejected) {[m
[32m+[m[32m  this.handlers.push({[m
[32m+[m[32m    fulfilled: fulfilled,[m
[32m+[m[32m    rejected: rejected[m
[32m+[m[32m  });[m
[32m+[m[32m  return this.handlers.length - 1;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Remove an interceptor from the stack[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Number} id The ID that was returned by `use`[m
[32m+[m[32m */[m
[32m+[m[32mInterceptorManager.prototype.eject = function eject(id) {[m
[32m+[m[32m  if (this.handlers[id]) {[m
[32m+[m[32m    this.handlers[id] = null;[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Iterate over all the registered interceptors[m
[32m+[m[32m *[m
[32m+[m[32m * This method is particularly useful for skipping over any[m
[32m+[m[32m * interceptors that may have become `null` calling `eject`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Function} fn The function to call for each interceptor[m
[32m+[m[32m */[m
[32m+[m[32mInterceptorManager.prototype.forEach = function forEach(fn) {[m
[32m+[m[32m  utils.forEach(this.handlers, function forEachHandler(h) {[m
[32m+[m[32m    if (h !== null) {[m
[32m+[m[32m      fn(h);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = InterceptorManager;[m
