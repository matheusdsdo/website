[32m+[m[32m/***/ }),[m
[32m+[m[32m/* 23 */[m
[32m+[m[32m/***/ (function(module, exports) {[m
[32m+[m
[32m+[m	[32m'use strict';[m
[32m+[m[41m	[m
[32m+[m	[32m/**[m
[32m+[m	[32m * A `Cancel` is an object that is thrown when an operation is canceled.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @class[m
[32m+[m	[32m * @param {string=} message The message.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction Cancel(message) {[m
[32m+[m	[32m  this.message = message;[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32mCancel.prototype.toString = function toString() {[m
[32m+[m	[32m  return 'Cancel' + (this.message ? ': ' + this.message : '');[m
[32m+[m	[32m};[m
[32m+[m[41m	[m
[32m+[m	[32mCancel.prototype.__CANCEL__ = true;[m
[32m+[m[41m	[m
[32m+[m	[32mmodule.exports = Cancel;[m
[32m+[m
[32m+[m
[32m+[m[32m/***/ }),[m
[32m+[m[32m/* 24 */[m
[32m+[m[32m/***/ (function(module, exports, __webpack_require__) {[m
[32m+[m
[32m+[m	[32m'use strict';[m
[32m+[m[41m	[m
[32m+[m	[32mvar Cancel = __webpack_require__(23);[m
[32m+[m[41m	[m
[32m+[m	[32m/**[m
[32m+[m	[32m * A `CancelToken` is an object that can be used to request cancellation of an operation.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @class[m
[32m+[m	[32m * @param {Function} executor The executor function.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction CancelToken(executor) {[m
[32m+[m	[32m  if (typeof executor !== 'function') {[m
[32m+[m	[32m    throw new TypeError('executor must be a function.');[m
[32m+[m	[32m  }[m
[32m+[m[41m	[m
[32m+[m	[32m  var resolvePromise;[m
[32m+[m	[32m  this.promise = new Promise(function promiseExecutor(resolve) {[m
[32m+[m	[32m    resolvePromise = resolve;[m
[32m+[m	[32m  });[m
[32m+[m[41m	[m
[32m+[m	[32m  var token = this;[m
[32m+[m	[32m  executor(function cancel(message) {[m
[32m+[m	[32m    if (token.reason) {[m
[32m+[m	[32m      // Cancellation has already been requested[m
[32m+[m	[32m      return;[m
[32m+[m	[32m    }[m
[32m+[m[41m	[m
[32m+[m	[32m    token.reason = new Cancel(message);[m
[32m+[m	[32m    resolvePromise(token.reason);[m
[32m+[m	[32m  });[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Throws a `Cancel` if cancellation has been requested.[m
[32m+[m	[32m */[m
[32m+[m	[32mCancelToken.prototype.throwIfRequested = function throwIfRequested() {[m
[32m+[m	[32m  if (this.reason) {[m
[32m+[m	[32m    throw this.reason;[m
[32m+[m	[32m  }[m
[32m+[m	[32m};[m
[32m+[m[41m	[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Returns an object that contains a new `CancelToken` and a function that, when called,[m
[32m+[m	[32m * cancels the `CancelToken`.[m
[32m+[m	[32m */[m
[32m+[m	[32mCancelToken.source = function source() {[m
[32m+[m	[32m  var cancel;[m
[32m+[m	[32m  var token = new CancelToken(function executor(c) {[m
[32m+[m	[32m    cancel = c;[m
[32m+[m	[32m  });[m
[32m+[m	[32m  return {[m
[32m+[m	[32m    token: token,[m
[32m+[m	[32m    cancel: cancel[m
[32m+[m	[32m  };[m
[32m+[m	[32m};[m
[32m+[m[41m	[m
[32m+[m	[32mmodule.exports = CancelToken;[m
[32m+[m
[32m+[m
[32m+[m[32m/***/ }),[m
[32m+[m[32m/* 25 */[m
[32m+[m[32m/***/ (function(module, exports) {[m
[32m+[m
[32m+[m	[32m'use strict';[m
[32m+[m[41m	[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Syntactic sugar for invoking a function and expanding an array for arguments.[m
[32m+[m	[32m *[m
[32m+[m	[32m * Common use case would be to use `Function.prototype.apply`.[m
[32m+[m	[32m *[m
[32m+[m	[32m *  ```js[m
[32m+[m	[32m *  function f(x, y, z) {}[m
[32m+[m	[32m *  var args = [1, 2, 3];[m
[32m+[m	[32m *  f.apply(null, args);[m
[32m+[m	[32m *  ```[m
[32m+[m	[32m *[m
[32m+[m	[32m * With `spread` this example can be re-written.[m
[32m+[m	[32m *[m
[32m+[m	[32m *  ```js[m
[32m+[m	[32m *  spread(function(x, y, z) {})([1, 2, 3]);[m
[32m+[m	[32m *  ```[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Function} callback[m
[32m+[m	[32m * @returns {Function}[m
[32m+[m	[32m */[m
[32m+[m	[32mmodule.exports = function spread(callback) {[m
[32m+[m	[32m  return function wrap(arr) {[m
[32m+[m	[32m    return callback.apply(null, arr);[m
[32m+[m	[32m  };[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m
[32m+[m[32m/***/ }),[m
[32m+[m[32m/* 26 */[m
[32m+[m[32m/***/ (function(module, exports) {[m
[32m+[m
[32m+[m	[32m'use strict';[m
[32m+[m[41m	[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Determines whether the payload is an error thrown by Axios[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {*} payload The value to test[m
[32m+[m	[32m * @returns {boolean} True if the payload is an error thrown by Axios, otherwise false[m
[32m+[m	[32m */[m
[32m+[m	[32mmodule.exports = function isAxiosError(payload) {[m
[32m+[m	[32m  return (typeof payload === 'object') && (payload.isAxiosError === true);[m
[32m+[m	[32m};[m
[32m+[m
