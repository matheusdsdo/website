[32m+[m[41m	[m
[32m+[m	[32m      // Clean up request[m
[32m+[m	[32m      request = null;[m
[32m+[m	[32m    };[m
[32m+[m[41m	[m
[32m+[m	[32m    // Handle low level network errors[m
[32m+[m	[32m    request.onerror = function handleError() {[m
[32m+[m	[32m      // Real errors are hidden from us by the browser[m
[32m+[m	[32m      // onerror should only fire if it's a network error[m
[32m+[m	[32m      reject(createError('Network Error', config, null, request));[m
[32m+[m[41m	[m
[32m+[m	[32m      // Clean up request[m
[32m+[m	[32m      request = null;[m
[32m+[m	[32m    };[m
[32m+[m[41m	[m
[32m+[m	[32m    // Handle timeout[m
[32m+[m	[32m    request.ontimeout = function handleTimeout() {[m
[32m+[m	[32m      var timeoutErrorMessage = 'timeout of ' + config.timeout + 'ms exceeded';[m
[32m+[m	[32m      if (config.timeoutErrorMessage) {[m
[32m+[m	[32m        timeoutErrorMessage = config.timeoutErrorMessage;[m
[32m+[m	[32m      }[m
[32m+[m	[32m      reject(createError(timeoutErrorMessage, config, 'ECONNABORTED',[m
[32m+[m	[32m        request));[m
[32m+[m[41m	[m
[32m+[m	[32m      // Clean up request[m
[32m+[m	[32m      request = null;[m
[32m+[m	[32m    };[m
[32m+[m[41m	[m
[32m+[m	[32m    // Add xsrf header[m
[32m+[m	[32m    // This is only done if running in a standard browser environment.[m
[32m+[m	[32m    // Specifically not if we're in a web worker, or react-native.[m
[32m+[m	[32m    if (utils.isStandardBrowserEnv()) {[m
[32m+[m	[32m      // Add xsrf header[m
[32m+[m	[32m      var xsrfValue = (config.withCredentials || isURLSameOrigin(fullPath)) && config.xsrfCookieName ?[m
[32m+[m	[32m        cookies.read(config.xsrfCookieName) :[m
