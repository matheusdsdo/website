[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./../utils');[m
[32m+[m
[32m+[m[32mmodule.exports = ([m
[32m+[m[32m  utils.isStandardBrowserEnv() ?[m
[32m+[m
[32m+[m[32m  // Standard browser envs support document.cookie[m
[32m+[m[32m    (function standardBrowserEnv() {[m
[32m+[m[32m      return {[m
[32m+[m[32m        write: function write(name, value, expires, path, domain, secure) {[m
[32m+[m[32m          var cookie = [];[m
[32m+[m[32m          cookie.push(name + '=' + encodeURIComponent(value));[m
[32m+[m
[32m+[m[32m          if (utils.isNumber(expires)) {[m
[32m+[m[32m            cookie.push('expires=' + new Date(expires).toGMTString());[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (utils.isString(path)) {[m
[32m+[m[32m            cookie.push('path=' + path);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (utils.isString(domain)) {[m
[32m+[m[32m            cookie.push('domain=' + domain);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (secure === true) {[m
[32m+[m[32m            cookie.push('secure');[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          document.cookie = cookie.join('; ');[m
[32m+[m[32m        },[m
[32m+[m
[32m+[m[32m        read: function read(name) {[m
[32m+[m[32m          var match = document.cookie.match(new RegExp('(^|;\\s*)(' + name + ')=([^;]*)'));[m
[32m+[m[32m          return (match ? decodeURIComponent(match[3]) : null);[m
[32m+[m[32m        },[m
[32m+[m
[32m+[m[32m        remove: function remove(name) {[m
[32m+[m[32m          this.write(name, '', Date.now() - 86400000);[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m[32m    })() :[m
[32m+[m
[32m+[m[32m  // Non standard browser env (web workers, react-native) lack needed support.[m
[32m+[m[32m    (function nonStandardBrowserEnv() {[m
[32m+[m[32m      return {[m
[32m+[m[32m        write: function write() {},[m
[32m+[m[32m        read: function read() { return null; },[m
[32m+[m[32m        remove: function remove() {}[m
[32m+[m[32m      };[m
[32m+[m[32m    })()[m
[32m+[m[32m);[m
[1mdiff --git a/servidor/node_modules/axios/lib/helpers/deprecatedMethod.js b/servidor/node_modules/axios/lib/helpers/deprecatedMethod.js[m
[1mnew file mode 100644[m
[1mindex 0000000..ed40965[m
