[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Creates a new URL by combining the specified URLs[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} baseURL The base URL[m
[32m+[m[32m * @param {string} relativeURL The relative URL[m
[32m+[m[32m * @returns {string} The combined URL[m
[32m+[m[32m */[m
[32m+[m[32mmodule.exports = function combineURLs(baseURL, relativeURL) {[m
[32m+[m[32m  return relativeURL[m
[32m+[m[32m    ? baseURL.replace(/\/+$/, '') + '/' + relativeURL.replace(/^\/+/, '')[m
[32m+[m[32m    : baseURL;[m
[32m+[m[32m};[m
