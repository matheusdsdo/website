[32m+[m
[32m+[m	[32m'use strict';[m
[32m+[m[41m	[m
[32m+[m	[32mvar utils = __webpack_require__(2);[m
[32m+[m[41m	[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Config-specific merge-function which creates a new config-object[m
[32m+[m	[32m * by merging two configuration objects together.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Object} config1[m
